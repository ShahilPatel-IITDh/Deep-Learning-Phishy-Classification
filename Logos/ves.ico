/* trials meta nav */
:root {
    --b-c: #0e6eb9;
    --b-bc: #0e6eb9;
    --b-c_h: #0b5995;
    --b-bc_h: #0b5995;
}

.meta-nav__item:nth-of-type(4) .meta-nav__link,
.u09-mobile-wrapper > .nav-list > .nav-list__item:nth-last-of-type(2) > .nav-list__link {
    color: #fff;
    background-color: var(--b-c);
    border-color: var(--b-bc);
    animation: .1s color_change
}

.meta-nav__item:nth-of-type(4) .meta-nav__link:hover,
.u09-mobile-wrapper > .nav-list > .nav-list__item:nth-last-of-type(2) > .nav-list__link:hover {
