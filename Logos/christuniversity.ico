function openCourse(val,camId)
{
	var divisionID = $('#divisionIdHidden').val();
	$.ajax({
		type: "POST",
 		url: "https://christuniversity.in/php/ajax/getCourse.php",
   		data: 'dept_id='+val+'&divisionID='+divisionID+'&camId='+camId,
  		cache: false,
  		success: function(msg){ //alert(msg);
			var mss=msg.split('~');
			if($('#departmentName1').text()!=mss[0])
			{
				$(".panel2").css('display','block');	
			if($('.panel2').is(":visible"))
			{
			 $('html, body').animate({
                    scrollTop: $('#courses').offset().top
                }, 1000);
			}
			else
			{
				$(".panel2").toggle();
				 $('html, body').animate({
                    scrollTop: $('#courses').offset().top
                }, 1000);
			}
			}
			else
			{
				$(".panel2").toggle();
				if($('.panel2').is(":visible"))
				{
				 $('html, body').animate({
                    scrollTop: $('#courses').offset().top
                }, 1000);
				}
			}
			$('#departmentName').html(mss[0]);
			$('#departmentName1').html(mss[0]);
			$('#deptIntro').html(mss[1]);
			$('#courseCatOffered').html(mss[2]);
			$("#courseCatImg").attr("src",mss[3]);
			if(mss[4]!='')
			{
			$('.deptTestimonials').html(mss[4]);
			$('.testm').show();
			}
			else
			{
				$('.testm').hide();
			}
			$('#exploreDepts').html('<a href="'+mss[5]+'" class="btn btn-default expl-bttns"> Explore </a>');
		},
		error: function(){
			alert('Server Busy');

		}
 	});		
}
</script>
