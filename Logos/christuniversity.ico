	//$('.panel').hide();
	//$("#panel_"+divVal).toggle();
	}
	/*new section --28-9-2020 by divu*/
	var click = +$(this).data('clicks') || 0;
    if (click % 2 == 1) {
     $("#panel_"+divVal).toggle();
    }else{
     // $("#panel_"+divVal).css('display','block');
    };
    $(this).data('clicks',click+1);
	/*ends here*/
	$.ajax({
		type: "POST",
 		url: "https://christuniversity.in/php/ajax/getDepartments.php",
   		data: 'division_id='+val+'&campusId='+campusId,
