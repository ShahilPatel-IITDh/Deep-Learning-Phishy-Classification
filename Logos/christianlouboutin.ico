    function prepareCompass(){
        if( !(navigator.userAgent.match(/(iPad|iPhone|iPod)/g)) ){
            Event.observe(window, 'mousemove', function(e){
                var compass = $('modal_compass_arrow');
                var compass_layout = compass.getLayout();
                var offset = compass.viewportOffset();
                var center_x = (offset.left) + (compass.measure('width') / 2);
                var center_y = (offset.top) + (compass.measure('height') / 2);
                var radians = Math.atan2(e.pageX - center_x, e.pageY - center_y);
                var degree = (radians * (180 / Math.PI) * -1) + 180;
                TweenLite.to(compass, 0, {rotation:degree});
            });
        }
    }
