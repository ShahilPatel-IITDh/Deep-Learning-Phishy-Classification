    <div class="newsbody"></div>
    <!-- /.newsbody -->
    <script>
var gNewsJsonUrl = "json/news.json";
// キャッシュを防ぐ為、後ろに日付を連結
var date = new Date();
gNewsJsonUrl += "?" + date;


    $(function() {
     $.getJSON(gNewsJsonUrl, function(top_news) {
      for (var i in top_news) {

		var dateStr = top_news[i].display_date_from;
		dateStr = dateStr.replace(/\//g,".");




		var tempStr = "";
		var tempVal = 0;

		var nowDate = new Date();
		var nowDateStr = "";
		nowDateStr += "" + nowDate.getFullYear();

		tempStr = "0" + (nowDate.getMonth() + 1);
		tempVal = tempStr.length - 2;
		tempStr = tempStr.substr(tempVal,tempVal + 2);
		nowDateStr += tempStr;

		tempStr = "0" + nowDate.getDate();
		tempVal = tempStr.length - 2;
		tempStr = tempStr.substr(tempVal,tempVal + 2);
		nowDateStr += tempStr;

		nowDateVal = nowDateStr * 1;

		var displayDateFrom = top_news[i].display_date_from;
		
		if(displayDateFrom != ""){
			tempStr = displayDateFrom.replace(/\//g,"");
			tempVal = tempStr * 1;
			
			if(nowDateVal < tempVal){
				// まだ開始日に達していない
				continue;
			}
		}
		var displayDateTo = top_news[i].display_date_to;
		if(displayDateTo != ""){
			tempStr = displayDateTo.replace(/\//g,"");
			tempVal = tempStr * 1;
			
			if(tempVal < nowDateVal){
				// もう終了日を過ぎている
				continue;
			}
		}




		var newsKbn = top_news[i].news_kbn;
		var newsKbnStr = "";
		switch(newsKbn){
		case "1":
			// 商品情報	1
			newsKbnStr = "商品情報";
			break;
		case "2":
			newsKbnStr = "店舗情報";
			break;
		case "9":
			newsKbnStr = "その他";
			break;
		default:
			newsKbnStr = "その他";
			break;
		}

		var newsLink = "oshirase.html?" + top_news[i].news_id;

//       var h = '<dl>' + '<dt>' + '<span class="date">' + top_news[i].date + '</span>' + '<span class="newscat">' + top_news[i].cat + '</span>' + '</dt>' + '<dd>' + '<a href="' + top_news[i].link + '">' + top_news[i].title + '</a>' + '</dd>' + '</dl>';
//       $(".newsbody").append(h);


       var h = '';
       h += '<dl>' + '<dt>' + '\n';
       h += '<span class="date">' + dateStr + '</span>' + '\n';
       h += '<span class="newscat">' + newsKbnStr + '</span>' + '\n';
       h += '</dt>' + '<dd>' + '\n';
       h += '<a href="' + newsLink + '">' + top_news[i].news_title + '</a>' + '</dd>' + '</dl>';
       $(".newsbody").append(h);


      }
     });
    });
    </script>

