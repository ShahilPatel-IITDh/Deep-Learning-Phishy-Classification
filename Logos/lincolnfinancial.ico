    <script type="text/javascript">
      function getAnalyticsEnv() {
        var host = window.location.host;
        if (host.includes("-dev") || host.includes("localhost")) {
          return "dev";
        }
        if (
          host.includes("-test") ||
          host.includes("-uat") ||
          host.includes("-preprod")
        ) {
          return "qa";
        }
        return "prod";
      }
      (function (a, b, c, d) {
        a =
          "https://tags.tiqcdn.com/utag/lfg/lfgrwdbasic/" +
          getAnalyticsEnv() +
          "/utag.js";
        b = document;
        c = "script";
        d = b.createElement(c);
        d.src = a;
        d.type = "text/java" + c;
        d.async = true;
        a = b.getElementsByTagName(c)[0];
        a.parentNode.insertBefore(d, a);
      })();
      
      
    </script>
    <script src="https://idm.web.lfg.com/idm-login/runtime.js" defer></script>
