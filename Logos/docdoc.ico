<script>var el=document.getElementById('js-header');
if(el){setTimeout(function(){el.classList.add('is-visible');},500);};
var hamburger = document.querySelector('.js-hamburger') || false,
	menu = document.querySelector('.menu-mobile') || false,
	overlay = document.querySelector('.menu-mobile__overlay') || false,
	btnClose= document.querySelector('.menu-mobile__close') || false;

if ( hamburger) {
	hamburger.addEventListener('click', function (e) {
		e.stopPropagation();
		menu.classList.add('is-open');
		overlay.classList.add('is-open');
	});
}

if ( menu ) {
	menu.addEventListener('click', function (e) {
		e.stopPropagation();
	});
}
if ( overlay && btnClose ) {	
	[overlay, btnClose].forEach(function (element) {
		element.addEventListener('click', function () {
			menu.classList.remove('is-open');
			overlay.classList.remove('is-open');
		});
	});
}</script>
