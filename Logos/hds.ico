    position: relative;
    transition: 0.5s;
    border-radius: 20px;
    background: #fff;
}


@keyframes img-fade{
    0% {
        opacity: 0;
    }

    25% {
        opacity: 0;
    }

    50% {
        opacity: 0.3;
    }


    100% {
        opacity: 1;
    }
}


@keyframes img-fade-revers{
    0% {
        opacity: 0 1;
    }

    25% {
        opacity: 0.3;
    }

    50% {
        opacity: 0;
    }


    100% {
        opacity: 0;
    }
