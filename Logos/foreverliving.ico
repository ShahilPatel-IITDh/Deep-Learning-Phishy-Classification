<script type="text/javascript">

function setCookie(cname, cvalue, exdays) {
      var d = new Date();
      d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
      var expires = "expires=" + d.toUTCString();
      document.cookie = cname + "=" + cvalue + ";expires=" + expires + ";domain=" + apiConfig.cookieConfig.domain + ";path=/";
    }

    function addFontsTemplateStyle(config) {
      var style = document.createElement('link');
      style.href = 'https://cdn-integ.flptitan.com' + "/css/V1/fonts.css";
      style.rel = 'stylesheet';
      document.head.appendChild(style);
    }

    function addFontAwesomeTemplateStyle(config) {
      var style = document.createElement('link');
      style.href = 'https://cdn-integ.flptitan.com' + "/css/V1/fontawesome.min.css";
      style.rel = 'stylesheet';
      document.head.appendChild(style);
    }

    function addRobotoFontTemplateStyle(config) {
      var style = document.createElement('link');
      style.href = 'https://cdn-integ.flptitan.com' + "/css/V1/roboto_fonts.css";
      style.rel = 'stylesheet';
      document.head.appendChild(style);
    }

    function addpfSignSkriptFontTemplateStyle(config) {
      var style = document.createElement('link');
      style.href = 'https://cdn-integ.flptitan.com' + "/css/V1/PfSignSkript-font.css";
      style.rel = 'stylesheet';
      document.head.appendChild(style);
    }
    function addBrokenImg(config) {
      var img = document.createElement("img");
      img.src = 'https://cdn-integ.flptitan.com' + '/images/page-broken.png';
      img.height = "100";
      document.getElementById("sso-brokenImg").appendChild(img);
    }

    function addForeverLogo(config) {
      var image = document.getElementById("foreverlogo");
      if(image){
        image.src = "https://cdn-integ.flptitan.com/flpi/logo/foreverliving-eCom-logo.svg";
      }
    }

