  function getConfigData(data) {
    apiConfig = data;
    initializeCookieConsent(apiConfig);
    document.querySelector("link[rel*='icon']").href = 'https://cdn-integ.flptitan.com' + "/flpi"+
      "/logo/favicon.ico";
      addAccountCmsTemplateStyle(apiConfig);
      addCmsTemplateStyle(apiConfig);
       addShopCmsTemplateStyle(apiConfig);
     //addReportsCmsTemplateStyle(apiConfig);
      addBrokenImg(apiConfig);
      addForeverLogo(apiConfig);
     // addLoaderImg(apiConfig);
      addFontAwesomeTemplateStyle(apiConfig);
      addFontsTemplateStyle(apiConfig);
      addRobotoFontTemplateStyle(apiConfig);
      if(data.enabledPfScript){
        addpfSignSkriptFontTemplateStyle(apiConfig);
      }
    if (data.languageCultureCode) {
      localStorage.setItem("languageCultureCode", JSON.stringify(data.languageCultureCode));
    }

    var exData = JSON.parse(localStorage.getItem("eCommerce.Configuration"));
    if (!localStorage.getItem("eCommerce.Configuration") ||
      (!JSON.stringify(exData.countryName) && !JSON.stringify(exData.templateConfiguration))) {
      localStorage.setItem("eCommerce.Configuration", JSON.stringify(data));
      //localStorage.setItem("eCommerce.countryName", 'USA');
    }
  }
</script>


