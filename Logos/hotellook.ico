<script>
  
  if(['/privacy', '/cookie', '/terms'].includes(window.location.pathname)){
      document.querySelector('.TPWL-front-content').setAttribute('style', 'max-width: 970px;margin:auto;');
  };
  
  
  (function(){
    function HLGetCookie(e) {
      let c = e + "=",
        d = decodeURIComponent(document.cookie).split(";");
      for (let b = 0; b < d.length; b++) {
        let a = d[b];
        for (;
          " " == a.charAt(0);) a = a.substring(1);
        if (0 == a.indexOf(c)) return a.substring(c.length, a.length)
      }
      return ""
    };

    var HLMarker = new URLSearchParams(window.location.search).get('marker') || HLGetCookie('marker') || 'direct';
    if (!!HLMarker.match(/^\d{5}/)) {
      var appstoreLinkNode = document.getElementById('appstore_link');
      var googleplayLinkNode = document.getElementById('googleplay_link');
      HLMarker = encodeURIComponent(decodeURIComponent(HLMarker));

      appstoreLinkNode.setAttribute('href', "http://www.hotellook.com/ios/?marker=" + HLMarker);
      googleplayLinkNode.setAttribute('href', "http://www.hotellook.com/android/?marker=" + HLMarker);
    }
  })();
</script>
