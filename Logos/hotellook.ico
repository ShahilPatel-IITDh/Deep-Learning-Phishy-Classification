.footer {
  position: relative;
  width: 100%;
  padding-top: 30px;
  background-color: #fff;
}
.footer .widgets {
  box-sizing: border-box;
  height: 130px;
  padding: 25px 0;
  display: -ms-flexbox;
  display: flex;
  flex-deirection: row;
  -ms-flex-pack: justify;
  justify-content: space-between;
  border-bottom: 1px solid rgba(0,0,0,.07);
}

.footer .footer-row {
  width: 994px;
  margin: 0 auto;
}

.footer .info {
  width: 274px;
}

.footer .copyright {
  display: inline-block;
  padding-bottom: 10px;
}
.footer .footer-title {
  font-family: "ProximaNova-Semibold",Helvetica,sans-serif;
  font-size: 13px;
  line-height: 16px;
}
.footer .description {
  display: inline-block;
  font-size: 13px;
