            blankDevice= 'phone';
            blankPc.hide();
        }else{
            blankDevice= 'pc';
            blankPhone.hide();
        }
        //- resize時
        $(window).resize(function(){
            if($(window).width() < 768 && blankDevice == 'pc'){
                blankPc.hide();
                blankPhone.show();
                blankDevice= 'phone';
            }
            if($(window).width() >= 768 && blankDevice == 'phone'){
