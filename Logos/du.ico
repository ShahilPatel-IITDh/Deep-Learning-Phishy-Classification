<script>
var readCookie = function (cname) {
   var name = cname + "=";
   var ca = document.cookie.split(';');
   for (var i = 0; i < ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0) == ' ') {
         c = c.substring(1);
      }
      if (c.indexOf(name) == 0) {
         return c.substring(name.length, c.length);
      }
   }
   return "";
};   
var loggeduser = readCookie('username');
var siteUrl = readCookie('siteUrl');
	if(loggeduser != "" && siteUrl  != "") {
        document.getElementById("nonlogin").style.display = "none";
        document.getElementById("myaccount_link").style.display = "block";        
        document.getElementById('loginuser').innerText = loggeduser;
        if(document.getElementsByTagName('html')[0].dir != null && document.getElementsByTagName('html')[0].dir != ""){
            if(document.getElementsByTagName('html')[0].dir == "rtl")
            {
               document.getElementById('myaccount_link').href = siteUrl;
               document.getElementById('myaccount-mobile-logged').href = siteUrl;
                
            }else{
               document.getElementById('myaccount_link').href = siteUrl;
               document.getElementById('myaccount-mobile-logged').href = siteUrl;
            }
        }
    }
</script>
<script>
     var searchIcon = document.querySelector('.du-search-box .sicon');
     searchIcon.addEventListener('click', function () {
     console.log('I am ')
    function processChanges(){
        console.log('Clicked')
    }

     var url = "https://www.du.ae/servlet/duaediscovery/common/km/common/static/helpandsupport/";
     var url2 = "https://help.du.ae/system/ws/v11/ss/";
     var portalID = "201600000001018";
     var portalID2 = "201600000001020";

     var isArabic = document.getElementsByTagName('html')[0].getAttribute('dir') === 'rtl';


     var localeEn = "en-US";
     var localeAr = "ar-SA";

     function getkmSession() {
         var varPortalId = isArabic? portalID2: portalID;
         fetch("https://help.du.ae/system/ws/v15/ss/portal/"+varPortalId+"/authentication/anonymous", {
             method: "POST",
             headers: {
                 'Accept': 'application/json',
                 "Content-Type": "application/json",
                 'Accept-Language': isArabic? localeAr: localeEn
             }
         })
             .then((response) => {
                 response.headers.forEach(function(val, key) {
                     if (key == "x-egain-session") {
                         setCookie('egainsessionmain', val);
                     }
                 });
             })
             .catch((error) => {
                 console.log('Error while retrieving session: ' + error);
             });
     }

     function kmSession() {
         if(getCookie('egainsessionmain') ==='' || getCookie('egainsessionmain') === "undefined" ) {
             getkmSession();
         } else {
             console.log(getCookie('egainsessionmain'));
         }
     }

     function setCookie(cname,cvalue) {
         var d = new Date();
         d.setTime(d.getTime() + (5*60*1000));
         var expires = "expires=" + d.toGMTString();
         document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
     }

     function getCookie(cname) {
         var name = cname + "=";
         var decodedCookie = decodeURIComponent(document.cookie);
         var ca = decodedCookie.split(';');
         for(var i = 0; i < ca.length; i++) {
             var c = ca[i];
             while (c.charAt(0) == ' ') {
                 c = c.substring(1);
             }
             if (c.indexOf(name) == 0) {
                 return c.substring(name.length, c.length);
             }
         }
         return "";
     }

     function getParameterByName(name, url) {
         if (!url) url = window.location.href;
         name = name.replace(/[[\]]/g, "$&");
         var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
             results = regex.exec(url);
         if (!results) return null;
         if (!results[2]) return '';
         return decodeURIComponent(results[2].replace(/\+/g, " "));
     }

     function getCombinedResult(query) {

         kmSession(); //set km Session for support result
         var supportURL = isArabic ? url2+'search/kb?portalId='+portalID2+'&usertype=customer&$pagesize=4&resourceType=article&lang=&lang=ar-SA&q='+query : url2+'search/kb?portalId='+portalID+'&usertype=customer&$pagesize=4&resourceType=article&q='+query
         var pageURL =  'https://www.googleapis.com/customsearch/v1?key=AIzaSyAuaL-H_N1zCuCN9N7QeKP_mVWtTQml9a8&cx=000164462090333349328:3ljgfaxwcze&num=4&q='+query;

         Promise.all([
             fetch(supportURL, {
                 method: "GET",
                 headers: {
                     'Accept': 'application/json',
                     'X-egain-session':getCookie('egainsessionmain')
                 }
             }),
             fetch(pageURL,{
                 method: "GET"
             })
         ]).then(function (responses) {
             return Promise.all(responses.map(function (response) {
                 return response.json();
             }));
         }).then(function (data) {
             //document.querySelector('.js-search-results .js-loader-box').classList.add('hide');
             //$('.js-search-results .js-loader-box').hide();
             var combined;

             if (data[0].article.length > 0 && data[1].items.length != undefined) {
                 // combined = data[1].items.concat(data[0].article)
                 var arr1 = data[1].items;
                 var n1 = data[1].items.length;

                 var arr2 = data[0].article;
                 var n2 = data[0].article.length;
                 var gLinks = document.querySelector('.gLinks');
                 var articlesLinks = document.querySelector('.articlesLinks')
                 gLinks.innerHTML = "";
                 articlesLinks.innerHTML = ""
                 //$('.articlesLinks').html("")
                 arr1.forEach(function (item){
                    // gLinks.append('<li ><a href="'+item.formattedUrl+'" class="link">'+item.title+'</a></li>')
                     gLinks.innerHTML += '<li ><a href="'+item.formattedUrl+'" class="link">'+item.title+'</a></li>'
                 })
                 arr2.forEach(function (item){
                    articlesLinks.innerHTML +='<li><a href="https://www.du.ae/personal/support-articledetail?artid='+item.alternateId+'" class="link">'+item.name+'</a></li>'
                 })

             }

         })
         // .catch(function (error) {
         //     // if there's an error, log it
         //     console.log(error);
         //     $('.js-search-results .js-loader-box').hide();
         //     $('.js-search-error').removeClass('hide');
         // });
     }
         var $input = document.getElementById('main-search-input');
         var timeout;

        var searchResults =  document.querySelectorAll('.search-result')
        var popularResults =  document.querySelectorAll('.popular-result')

         var keyUpEventHandler2 = function(event) {
             clearTimeout(timeout);
             if(!$input.value == '') {
                 timeout = setTimeout(function () {
                     console.log('debounce')
                     var _query = $input.value
                     getCombinedResult(_query);
                     searchResults.forEach(function(name){ name.classList.remove('hide') })
                     popularResults.forEach(function(name){ name.classList.add('hide') })


                 }, 500);
             }
             else {
                 showStaticLinks()
             }
         }

         $input.addEventListener('keyup', keyUpEventHandler2);

         document.querySelector('.scloseicon').addEventListener('click', function (){
             showStaticLinks()
         })



         function showStaticLinks(){
            popularResults.forEach(function(name){ name.classList.remove('hide') })
            searchResults.forEach(function(name){ name.classList.add('hide') })
};
       })
    </script>

