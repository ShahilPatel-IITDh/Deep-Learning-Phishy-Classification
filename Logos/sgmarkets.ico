    <!-- Define default configuration -->
    <script>
      function isProduction() {
        return window.location.hostname === 'www.sgmarkets.com' || window.location.hostname === 'www-preview.sgmarkets.com';
      }

      // used to redirect https://www.sgmarket.com url to https://www.sgmarkets.com
      if (window.location.hostname === 'www.sgmarket.com')
      {
        window.location.replace("https://www.sgmarkets.com");
      }

      window.tracking_config = {
        piwikTrackerUrl: isProduction() ? 'https://t-log.sgmarkets.com/' : 'https://sg-analytics-uat.fr.world.socgen/piwik/',
        piwikSiteId: isProduction() ? '170' : '877539670',
      };

      window.SGWTWidgetConfiguration = {
        environment: isProduction() ? 'production' : 'homologation',
      };

      function loadWidget(name) {
        const prefix = isProduction() ? 'https://sgwt-cdn-widgets.sgmarkets.com/widgets' : 'https://sgwt-widgets-dev.sgmarkets.com/widgets';
        const url = prefix + '/' + name;

        load_script(url, null);
      }

      function load_script(url, integrity) {
        var new_script = document.createElement('script');
        new_script.type = 'text/javascript';
        new_script.src = url;
        new_script.async = false;
        new_script.rel = 'preload';
        new_script.defer = true;
        if (integrity) {
          new_script.crossOrigin = 'anonymous';
          new_script.integrity = integrity;
        }

        document.getElementsByTagName('head')[0].appendChild(new_script);
      }

      loadWidget('sgwt-widget-polyfills/sgwt-widget-polyfill-webcomponent.js');
      load_script('https://code.jquery.com/jquery-3.2.1.slim.min.js', 'sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN');
      load_script('https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js', 'sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q');
      load_script('https://shared.sgmarkets.com/sg-bootstrap/v4/4.1.4/sg-bootstrap.min.js');
      loadWidget("sgwt-connect/v4/sgwt-connect.js");
      loadWidget('sgwt-account-center/v4/sgwt-account-center.js');
      loadWidget('sgwt-help-center/v4/sgwt-help-center.js');
      loadWidget('sgwt-mini-footer/v4/sgwt-mini-footer.js');
      loadWidget('sgwt-splash-screen/v4/sgwt-splash-screen.js');
      loadWidget('sgwt-web-analytics/v4/sgwt-web-analytics.js');
      load_script('https://shared.sgmarkets.com/sg-link/protocolcheck.js');
    </script>
