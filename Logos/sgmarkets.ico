  <!-- Piwik -->
  <script type="text/javascript">
    const element = document.createElement("sgwt-web-analytics");
    element.setAttribute("site-id", window.tracking_config.piwikSiteId);
    element.setAttribute("base-url", window.tracking_config.piwikTrackerUrl);
    document.body.appendChild(element);
  </script>
