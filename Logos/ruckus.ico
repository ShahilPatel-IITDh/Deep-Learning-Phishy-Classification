  <script>
      // Google's official recommendation for how to detect a specific webp feature
      function check_webp_feature(feature, callback) {
          var kTestImages = {
              lossy: "UklGRiIAAABXRUJQVlA4IBYAAAAwAQCdASoBAAEADsD+JaQAA3AAAAAA",
              lossless: "UklGRhoAAABXRUJQVlA4TA0AAAAvAAAAEAcQERGIiP4HAA==",
              alpha: "UklGRkoAAABXRUJQVlA4WAoAAAAQAAAAAAAAAAAAQUxQSAwAAAARBxAR/Q9ERP8DAABWUDggGAAAABQBAJ0BKgEAAQAAAP4AAA3AAP7mtQAAAA==",
              animation: "UklGRlIAAABXRUJQVlA4WAoAAAASAAAAAAAAAAAAQU5JTQYAAAD/////AABBTk1GJgAAAAAAAAAAAAAAAAAAAGQAAABWUDhMDQAAAC8AAAAQBxAREYiI/gcA"
          };
          var img = new Image();
          img.onload = function () {
              var result = (img.width > 0) && (img.height > 0);
              callback(feature, result);
          };
          img.onerror = function () {
              callback(feature, false);
          };
          img.src = "data:image/webp;base64," + kTestImages[feature];
      }

      check_webp_feature('alpha', function(feature, isSupported){
          if (!isSupported) {
              document.querySelector('body').classList.add('noWebp')
          }
      })


      $(function() {
          if ($(window).width() > 640) {
              if ($('.form-signin').outerHeight() < $('.login-messages').outerHeight()) {
                  $('.form-signin fieldset').css({'min-height': $('.login-messages').innerHeight() + 'px'});
              }
          }
          $('#read_notice').on('click', function() {
              $('#notice').show(300);
              $(this).hide();
          });
      });
  </script>
  <script>
(function($) {
  $(':submit').click(function(evt) {
    var btn = $(this),
        form = btn.parents('form')[0];
    evt.preventDefault();
    btn.prop('disabled', true).val('Working ...');
    setTimeout(function() {
      form.submit();
    }, 20);
  });

  if($("#referer").text() === "true" ) {
    $(':submit').click(function() {
      $('#waitModal').bind('opened', function() {
        $(".form-signin").submit();
      });
      $('#waitModal').foundation('reveal', 'open');
      var timer = 9;
      var textArray = { 6: "Generating kumo trial...", 5: "Registering your code...", 4: "Generating licenses...", 1: "Sending serial number to cloud...", 0: "Finalizing your registration..." };
      setInterval(function() {
        timer = timer - 1;
        if (timer < 0) timer = 0;
        if (textArray[timer] !== undefined) {
          $("#progress-text").text(textArray[timer]);
        }
      }, 5000);
    });
  }
})(jQuery);
</script>


  <div id="referer" style="display: none"></div>

  <script>
  (function () {
    'use strict';
    // Show password text
    const user_password = document.querySelector('#user_password');
    const eye = document.querySelector('#eye');
    eye.addEventListener('click', function(){
      this.classList.toggle('fa-eye-slash');
      this.classList.toggle('fa-eye');
      const current_type = user_password.getAttribute('type');
      user_password.setAttribute('type', current_type === 'password' ? 'text' : 'password');
    });

    // Disable submit button
    const form_signin = document.querySelector('.form-signin');
    const submit_btn = document.querySelector('.form-signin .btn-login');
    form_signin.addEventListener('submit', function(evt){
      submit_btn.value = 'Working ...';
      submit_btn.disabled = true;

      setTimeout(function () {
        form_signin.submit()
      }, 20);

      evt.preventDefault();
      return;
    });
  })();
  </script>
