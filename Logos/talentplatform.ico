				checklistMobileBot[0].style.display = 'none';
				checklistMobileBot[1].style.display = 'none';

				break;

			case 'Analyst recognition':

				document.getElementById("label-tab-4").click();
				break;

			case 'Thought leadership':

				document.getElementById("label-tab-5").click();
				break;

			case 'Events': // deprecated

				document.getElementById("label-tab-6").click();
				break;

			default:
		}
	};

	// Initialize the tabs using the path in the URL
	if(typeof pathArray[3] == 'string'){

		if(pathArray[3] != ''){

			switch(pathArray[3]){
				
				case 'news':

					document.getElementById("label-tab-2").click();
					break;

				case 'research': //research

					document.getElementById("label-tab-3").click();
					break;

				case 'analyst-recognition':

