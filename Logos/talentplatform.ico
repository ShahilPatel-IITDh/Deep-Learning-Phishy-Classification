					break;

				case 'thought-leadership':

					document.getElementById("label-tab-5").click();
					break;

				case 'events': // deprecated

					document.getElementById("label-tab-6").click();
					break;
				case 'page':

					paged = true;
					break;

				case 'archive':
					// nothing here
					break;
				default:
					break;
			}

		} else { // fired when page is initialize *without* overview in the URL

			document.getElementById("label-tab-1").click();
			document.querySelectorAll('.newsroom-overview').forEach(function(el) {
			  el.style.display = "block";
			});

		}
