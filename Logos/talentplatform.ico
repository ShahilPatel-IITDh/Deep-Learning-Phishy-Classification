<div id="newsroom_footer"></div>

<script>

	var paged = false;

			var baseTabURL = '/company/corporate-newsroom/';
	
	jQuery(document).ready(function($){

					var baseURL = '/company/corporate-newsroom/news/';
		
		$("#content-type").change(function(){
			var url = baseURL + "?press=" + $("#content-type").val() + "&year-date=" + $("#year").val() +  "#articles";
		 	window.location.replace(url);
		});

		$("#year").change(function(){
		 	window.location.replace(baseURL + "?press=" + $("#content-type").val() + "&year-date=" + $("#year").val() +  "#articles" );
		});

		
	});

	document.querySelectorAll('.newsroom-footer').forEach(function(el) {
		document.getElementById('newsroom_footer').append(el);
	});
	document.getElementById('buttonAll').onclick = function(){
		document.getElementById('content-type').options[0].selected = true;
		document.getElementById('content-type').dispatchEvent(new Event('change'));
	}
	document.getElementById('buttonMedia').onclick = function(){
		document.getElementById('content-type').options[1].selected = true;
		document.getElementById('content-type').dispatchEvent(new Event('change'));
	}
	document.getElementById('buttonPress').onclick = function(){
		document.getElementById('content-type').options[2].selected = true;
		document.getElementById('content-type').dispatchEvent(new Event('change'));
	}

	var mobileNewsNav = document.querySelectorAll('.mobile-nav-select')[0];
	
	// Get the Path from the URL & account for lang slug
	var pathArray = window.location.pathname.split('/');
	
	document.querySelectorAll('.module-tabbed-content a.nav-link').forEach(function(el) {

		switch(el.id){

			case 'label-tab-1': // Overview

				el.onclick = function(){

					document.querySelector('.news-archive').classList.remove('overview','news','research','analyst','events','blog');
					document.querySelector('.news-archive').classList.add('overview');

					var pageTitle = "iCIMS | Overview";
					window.history.pushState({pageTitle:pageTitle}, pageTitle, baseTabURL + 'overview/');
					document.title = pageTitle;
					mobileNewsNav.value = "Overview";

				};

				break;

			case 'label-tab-2': // News

				el.onclick = function(){

					document.querySelector('.news-archive').classList.remove('overview','news','research','analyst','events','blog');
					document.querySelector('.news-archive').classList.add('news');

					var pageTitle = "iCIMS | News";
					if(pathArray[3] != 'news'){ // do not drop query and page params
						window.history.pushState({pageTitle:pageTitle}, pageTitle, baseTabURL + 'news/');
					}
					document.title = pageTitle;
					mobileNewsNav.value = "News";

				};

				break;

			case 'label-tab-3': // Research

				el.onclick = function(){

					document.querySelector('.news-archive').classList.remove('overview','news','research','analyst','events','blog');
					document.querySelector('.news-archive').classList.add('research');

					var pageTitle = "iCIMS | Research";
					window.history.pushState({pageTitle:pageTitle}, pageTitle, baseTabURL + 'research/');
					document.title = pageTitle;
					mobileNewsNav.value = "Research";

				};

				break;

			case 'label-tab-4': // Analyst Recognition

				el.onclick = function(){

					document.querySelector('.news-archive').classList.remove('overview','news','research','analyst','events','blog');
					document.querySelector('.news-archive').classList.add('analyst');

					var pageTitle = "iCIMS | Analyst";
					window.history.pushState({pageTitle:pageTitle}, pageTitle, baseTabURL + 'analyst-recognition/');
					document.title = pageTitle;
					mobileNewsNav.value = "Analyst recognition";
					
				};

				break;

			case 'label-tab-5': // Throught Leadership (Blog)

				el.onclick = function(){

					document.querySelector('.news-archive').classList.remove('overview','news','research','analyst','events','blog');
					document.querySelector('.news-archive').classList.add('blog');

					var pageTitle = "iCIMS | Thought leadership";
					window.history.pushState({pageTitle:pageTitle}, pageTitle, baseTabURL + 'thought-leadership/');
					document.title = pageTitle;
					mobileNewsNav.value = "Thought leadership";

				};

				break;

			case 'label-tab-6': // Events (Deprecated)

				el.onclick = function(){
					window.location.href = "https://www.icims.com/resources/events/";
				};

				break;

			default:
		}
	});

	mobileNewsNav.onchange = function(){

		switch(this.value){

			case 'Overview':

				document.getElementById("label-tab-1").click();
				break;

			case 'News':

				document.getElementById("label-tab-2").click();
				break;

