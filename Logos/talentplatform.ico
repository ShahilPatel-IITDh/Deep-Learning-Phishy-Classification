                $( ".header-container" ).addClass( "scrolling" );
                $( "header" ).addClass( "scrolling" );
            } else {
                $( ".header-container" ).removeClass( "scrolling" );
                $( "header" ).removeClass( "scrolling" );
            }

        });
    });

