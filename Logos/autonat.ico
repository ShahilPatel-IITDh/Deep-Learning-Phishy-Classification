<!DOCTYPE html>
<html>
  <head>
	<title></title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" media="screen" />
    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Cache-control" content="no-cache">
    <meta http-equiv="Cache" content="no-cache">
    <link href="public/css/login.css?v=20230808.01" rel="stylesheet" type="text/css" />
	<script data-main="public/js/index.js?v=20230808.01" src="public/js/require.js" type="text/javascript"></script>
  </head>
  
  <body>
	<!-- page show area -->
	<div id="PageContent">
		<div id="loginContent">		
		    <div id="divLang">
		        <div id="langType">
		            <div id="langTypeSel"></div>
		            <span class="arrowDown"></span>
		            <div id="langTypeList"></div>
		        </div>
		    </div>
		    <div id="divCalendar">
		        <div id="calendarType">
		            <div id="calendarTypeSel">
		            </div>
		            <span class="arrowDown"></span>
		            <div id="calendarTypeList">
		            </div>
		        </div>
		    </div>
			<img id="mainBg" src="public/css/pictures/mainBg.png" />
			<div id="mainBgMask"></div>
			<div id="webName"></div>
			<div id="container">
				<div id="accountLoginWrap">
					<form id="loginForm" action="/servlet/SSO" method="post">
						<div class="itemWrap">
							<i class="icon sn-icon"></i>
							<input type="text" id="txtAccountSN" name="sn" value="" class="txt-input" autocomplete="off">
						</div>
						<ul id="snListStroage"></ul>
						<div class="itemWrap">
							<i class="icon user-icon"></i>
							<input id="txtUserName" class="txt-input" type="text"  /> 
						</div>
						<div class="itemWrap">
							<i class="icon pwd-icon"></i>
							<input id="txtPassword" class="txt-input" type="password" autocomplete="off" />
						</div>
						<div class="itemWrap">
							<button id="btnAccountLogin" class="cfgBtn" lc="html" lk="IDCS_LOGIN_NBSP"></button>
						</div>
					</form>
					<div id="ErrorMsg"></div>
				</div>
			</div>
		</div>
	</div>
  </body>
</html>
