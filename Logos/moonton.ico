                $(".homeC-b ul li").eq(this.activeIndex).addClass("cur").siblings().removeClass("cur");
            },
        },
    });
    $(".homeC-b ul li").click(function(){
        $(this).addClass("cur").siblings().removeClass("cur");
        $(".homeC-f .swiper-pagination .swiper-pagination-bullet").eq($(this).index()).trigger("click");
    })
    
    
    $(".bannerScroll").click(function() {
		$("html,body").animate({ scrollTop: $(window).height() }, 500);
	});
    
   
</script>


