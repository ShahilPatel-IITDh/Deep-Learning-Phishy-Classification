                  }
                });
                var listLength = $targetLists.length;
                if ((listLength % 2) != 0) {
                  $targetLists.last().addClass('twoTimesWidth');
                }
                if ((listLength % 2) == 0 && $targetLists.last().css('display') == 'none') {
                  $targetLists.last().prev().addClass('twoTimesWidth');
