    if(type == 1)
    {
        let now = new Date();
        let now_year = now.getFullYear();
        let now_month = now.getMonth() + 1;
        let _year_html = '';
        let _month_html = '';
        for (let start_year = 1940;start_year <= now_year;start_year++)
        {
            _year_html += start_year == now_year ? `<option value="${start_year}" selected>${start_year}</option>` : `<option value="${start_year}">${start_year}</option>`;
        }
        for (let i = 1;i<= 12;i++)
        {
            let month = i > 9 ? i : '0'+i;
            _month_html += month == now_month ? `<option value="${month}" selected>${month}</option>` : `<option value="${month}">${month}</option>`;
        }
        $('#edit_birthday_year').html(_year_html);
        $('#edit_birthday_month').html(_month_html);
        init_edit_birthday(0);
    }
    else
    {
        is_change_birthday++;
        let _day_html = '';
        let _month = Number($('#edit_birthday_month').val());
        let _this_month_day = 0;
        if([1,3,5,7,8,10,12].includes(_month))
        {
            _this_month_day = 31;
        }
        else if(_month == 2)
        {
            if($('#edit_birthday_year').val()%4 == 0)
            {
                _this_month_day = 29;
            }
            else
            {
                _this_month_day = 28;
            }
        }
        else
        {
            _this_month_day = 30;
        }
        for (let i = 1;i <= _this_month_day;i++)
        {
            let _day = i > 9 ? i : '0'+i;
            _day_html += `<option value="${_day}">${_day}</option>`;
        }
        $('#edit_birthday_day').html(_day_html);
    }
}
init_edit_birthday(1);
