  <script>
    function toggleGalleryModal() {
      $('.gallery-item').bind('click.openModal touchend.openModal', function() {
        let modalImage = $(this).find('img')[0].outerHTML;
        increaseRezolution(modalImage);
        $('body').toggleClass('modal-active');
      });
      $('.close-modal').bind('click.openModal touchend.openModal', function() {
        $('body').toggleClass('modal-active');
      });
    }

    function modalNavigationBind() {
      $('.modal-previous').bind('click.modalNavigate touchend.modalNavigate', function() {
        let prevImageNr = parseInt($('.modal-image img').attr('modal-img')) - 1;
        let prevImage = $('.gallery-item img[modal-img="'+(prevImageNr == 0 ? 6 : prevImageNr)+'"]')[0].outerHTML;
        increaseRezolution(prevImage);
      });

      $('.modal-next').bind('click.modalNavigate touchend.modalNavigate', function() {
        let nextImageNr = parseInt($('.modal-image img').attr('modal-img')) + 1;
        let nextImage = $('.gallery-item img[modal-img="'+(nextImageNr == 7 ? 1 : nextImageNr)+'"]')[0].outerHTML;
        increaseRezolution(nextImage);
      });
    }

    function mobileGalleryCarousel() {
      $('.galery-previous').bind('click.galleryNavigate touchend.galleryNavigate', function() {
        let prevImageNr = parseInt($('.gallery-item.mobile-active img').attr('modal-img')) - 1;
        $('.gallery-item').removeClass('mobile-active');
        $('.gallery-item img[modal-img="'+(prevImageNr == 0 ? 6 : prevImageNr)+'"]').parent().addClass('mobile-active');
        $('.gallery-count').text((prevImageNr == 0 ? 6 : prevImageNr)+' of 6');
      });

      $('.galery-next').bind('click.galleryNavigate touchend.galleryNavigate', function() {
        let nextImageNr = parseInt($('.gallery-item.mobile-active img').attr('modal-img')) + 1;
        $('.gallery-item').removeClass('mobile-active');
        $('.gallery-item img[modal-img="'+(nextImageNr == 7 ? 1 : nextImageNr)+'"]').parent().addClass('mobile-active');
        $('.gallery-count').text((nextImageNr == 7 ? 1 : nextImageNr)+' of 6');
      });
    }

    function increaseRezolution(img) {
      img = img.toString();
      let firstSplit = img.indexOf('png?');
      let secondSplit = img.indexOf('" alt');
      let firstBit = img.substring(0,firstSplit+4);
      let lastBit = img.substring(secondSplit);
      $('.modal-image').html(firstBit+'height=900&amp;width=1600'+lastBit);
    }

    toggleGalleryModal();
    modalNavigationBind();
    mobileGalleryCarousel();
  
  </script>

