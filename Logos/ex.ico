<script type="text/javascript">
	var exMail = exMail || {};

	exMail.initData  = {"login":null,"domain":{"flags":3,"host":"mail.ex.ua","fs":"mail.ex.ua","name":"ex.ua"},"captcha":0,"priv":0,"err":null};


	var SHOW_CAPTCHA = parseInt(exMail.initData.captcha);
	var	LOGIN = exMail.initData.login || 0;
	var	CORP = parseInt(exMail.initData.priv);
	var ERROR = {};

	

	try {
		ERROR = exMail.initData.err;
	} catch(e) {
		console.warn(e.message);
	}

	var $loading = document.getElementsByClassName('loading')[0];

	if(LOGIN) {
		var span = $loading.getElementsByTagName('span')[0];

		span.style.marginTop = window.innerHeight/2-100+'px';
		span.textContent = LOGIN+"@"+getHost();
		$loading.style.display = 'block';
		exMail.firstLoad = true;
		exMail.firstLoadPercent = 30;

		var startTime = (new Date()).getTime();

		

		exMail.loadingProgInterval = setInterval(function() {
			if(!exMail.firstLoad) {
				exMail.firstLoadPercent = 100;
				clearInterval(exMail.loadingProgInterval);
			}

			exMail.firstLoadPercent += getRandomInt(1, 5);
			if(exMail.firstLoadPercent < getRandomInt(90, 99)) {	
				document.getElementById('lpt').style.width = exMail.firstLoadPercent+'%';
			} else {
				document.getElementById('lpt').style.width = '100%';
			}

			// console.log('Loading...', exMail.firstLoadPercent);
		}, 100); 


		// ---

		

		function goLoadingCheck() {
			exMail.loadingInterval = setInterval(function() {
				if(!exMail.firstLoad) {
					$loading.style.display = 'none';
					clearInterval(exMail.loadingInterval);
				}

				var curTime = (new Date()).getTime();

				var diffTime = curTime-startTime;

				// console.log(diffTime);

				if((diffTime >= 4000) && (diffTime < 10000)) {
					clearInterval(exMail.loadingProgInterval);
					// console.log('%cTo slow', 'color: red; font-size: 20px;');
					exMail.firstLoadPercent += 0.5;
					if(exMail.firstLoadPercent < 100) {	
						document.getElementById('lpt').style.width = exMail.firstLoadPercent+'%';
					} else {
						document.getElementById('lpt').style.width = '100%';
					}
				} else if(diffTime >= 10000) {
					// console.log('%cWery slow', 'color: red; font-size: 20px;');
					exMail.firstLoadPercent += 0.09;
					if(exMail.firstLoadPercent < 100) {	
						document.getElementById('lpt').style.width = exMail.firstLoadPercent+'%';
					} else {
						document.getElementById('lpt').style.width = '100%';
					}
				}

			}, 100);
		}

		setTimeout(goLoadingCheck, 1500);

		function getRandomInt(min, max) {
		  return Math.floor(Math.random() * (max - min)) + min;
		}

		// ---
	} else {
		$loading.innerHTML = 'Загрузка...';
		$loading.style.paddingTop = '200px';
		$loading.style.display = 'block';
	}

	function getHost() {
		// var h = document.location.hostname;
		// var host = {
		// 	'mail.rover.info': 'rover.info',
		// 	'mail.r13.org': 'r13.org',
		// 	'mail.ex.ua': 'ex.ua'
		// };	
		// return host[h] || 'ex.ua';
		return exMail.initData.domain.name || 'ex.ua';
	}
</script>
