      toggled = false;
      $(".fop-navigation-menu-wrapper").removeClass("fop-navigation-menu-wrapper-open");
      $('html, body').css({
        overflow: 'auto',
      });
    }
  };
  
  $("#headerSearchModal").click((e) => {
    if (!document.getElementsByClassName('fop-search-form-inner')[0].contains(e.target)) {
    $("#smallSearchButton").removeClass("fop-none");
    $("#largeSearchButton").addClass("fop-none");
  }
                                });
  $('[name="kelime"]').click(() => {
    $("#smallSearchButton").addClass("fop-none");
    $("#largeSearchButton").removeClass("fop-none");
  });
    
    //search datalayer
    function triggerSearch() {
    var searchTerm = $('#searchFormInput').val();
    let datalayerObj = {
    'event': 'gaEvent',
    'eventCategory': 'Arama',
    'eventAction': 'Yapma',
    'eventLabel': searchTerm,
    'noninteraction': false,
  }                           
                             pushDataLayer(datalayerObj);
  var url = '/arama-sonuclari?kelime=' + encodeURIComponent(searchTerm);
  window.location = url;
  
  };
  $('#largeSearchButton').click(function (e) {
    e.preventDefault();
    triggerSearch();
  });
  $('[name="kelime"]').keypress(e => {
    if (e.which == 13) {
    e.preventDefault();
    triggerSearch()
  }
                                });
</script>
    
    

