    <div id="search-results-2" class=""
         v-show="query.length > 1 && pickerVisible">
        <div class="search-option" v-if="inProgress">Szukam ...</div>

        <div class="search-option" v-for="result in results"
             v-if="!inProgress">
            <div><a v-bind:href="result.link"
                    v-html="returnBold(result.title, query)"></a></div>
        </div>
        <div class="search-option" v-if="">Wybierz ENTER  lub kliknij <button class="btn-blue btn-insearch btn" style="" @click="window.location.replace('/wyszukiwarka' + '?query=' + query)"> Szukaj</button>
            aby przejść do dokładnego wyszukiwania
