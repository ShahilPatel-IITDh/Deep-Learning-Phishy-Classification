                @focus="pickerVisible = true"
                @blur="hideDialog()"
                v-bind:class="{ options: results.length > 0 && pickerVisible == true }"
                autocomplete="off"
                type="text"
                v-model="query"
                id="search"
                @keyup.enter="window.location.replace('/wyszukiwarka' + '?query=' + query)"
                class="w-100"
