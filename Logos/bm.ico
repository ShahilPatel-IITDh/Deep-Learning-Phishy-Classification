<script>
    window.searchNavbar = {
        show: function() {
            let element = document.querySelector("#navbarSearch");
            element.classList.add("is--active");
            element.animate({right: 0}, 500);
            element.style.right = 0;
        },
        hide: function() {
            let element = document.querySelector("#navbarSearch");
            element.classList.remove("is--active");
            element.animate({right: "-100%"}, 500);
            element.style.right = "-100%";
        }
    }



    document.addEventListener("DOMContentLoaded", () => {
      document.querySelector('#search-results-2').addEventListener("mouseover", (event) => {
          event.target.dataset.hover = 1;
      });
      document.querySelector('#search-results-2').addEventListener("mouseleave", (event) => {
          event.target.dataset.hover = 0;
      });
    });
    window.isHovering = function (selector) {
        return document.querySelector(selector).dataset.hover ? true : false; //check element for hover property
    }

    "use strict";

    var searchEngine2 = new Vue({
        delimiters: ['${', '}'],
        el: '#search-engine2',
        data: {
            'results': false,
            'query': '',
            'inProgress': 0,
            'queryParamName': 'query',
            'redirectUrl': null,
            'awaitingSearch': false,
            'pickerVisible': false
        },
        methods: {
            hideDialog: function () {
                //   console.log('hideDialog: ' + window.isHovering('#search-results'));
                if (window.isHovering('#search-results-2')) {
                    //console.log("hideDialog = true");
                    this.pickerVisible = true;
                } else {
                    //console.log("hideDialog = false");
                    this.pickerVisible = false;
                }
            },
            fetchQuery: function fetchQuery() {
                if (this.redirectUrl !== null) {
                    window.location = this.redirectUrl + "?" + this.queryParamName + "=" + this.query;
                    return;
                }

                this.inProgress++;
                this.$forceUpdate();
                //this.setLocation();
                fetch(encodeURI('/search-engine/search?per-page=5&searchBy=title&query=' + this.query)).then(function (resp) {
                    searchEngine2.inProgress--;
                    return resp.json();
                }).then(function (resp) {
                    searchEngine2.results = resp.items;
                });
            },
            setLocation: function setLocation() {
                var url = window.location.toString();
                var base = url.split("?")[0];
                window.history.pushState("search", "", base + "?" + this.queryParamName + "=" + this.query);
            },
            getDataParameter: function getDataParameter(dataParamName) {
                if (!document.querySelector('#search-engine2').hasAttribute(dataParamName)) {
                    return null;
                }

                return document.querySelector('#search-engine2').getAttribute(dataParamName);
            },
            returnBold: function (message, needle) {
                return message.replace(new RegExp(needle, 'gi'), '<b>$&</b>')
            },
            close(){
                window.searchNavbar.hide();
            }
        },
        created: function created() {
            this.queryParamName = this.getDataParameter('data-query-param-name');
            this.query = this.getDataParameter('data-query');
            this.redirectUrl = this.getDataParameter('data-redirect-url');

            if (this.query !== '' && this.query !== null) {
                this.fetchQuery();
            }
        },
        watch: {
            query: function query(val) {
                var _this = this;

                if (this.timer) {
                    clearTimeout(this.timer);
                    this.timer = null;
                }

                // first query without delay
                if (this.results === false) {
                    if (_this.query.length >= 2) {
                        _this.fetchQuery();
                    }
                    return;
                }

                this.timer = setTimeout(() => {

                    if (_this.query.length >= 2) {
                        _this.fetchQuery();
                    }

                    if (_this.query.length == 0) {
                        _this.results = {};
                    }

                }, 2000);
            }
        }
    });
</script>

