<script>
    function handleDropdownClick() {
        document.getElementById("languageDropdownMenu").classList.toggle("show");
        document.getElementById("languageInput").focus();
    }

    function filterFunction() {
        var input, filter, ul, li, a, i;
        input = document.getElementById("languageInput");
        filter = input.value.toUpperCase();
        div = document.getElementById("languageDropdownMenu");
        a = div.getElementsByTagName("a");
        for (i = 0; i < a.length; i++) {
            txtValue = a[i].textContent || a[i].innerText;
            if (txtValue.toUpperCase().indexOf(filter) > -1) {
                a[i].style.display = "";
            } else {
                a[i].style.display = "none";
            }
        }
    }

    function setLanguageCode() {
        var downTriangleCode = "&#9660";

        var langButton = document.getElementById("langButton");
        langButton.innerHTML = getLanguageCode() + "&nbsp;" + downTriangleCode;
    }

    function getLanguageCode() {
        var langCode = "EN";
        var splitPath = (window.location.pathname).split("/");
        if (splitPath.length > 2) {
            var lcidSymbol = splitPath[1];
            langCode = lcidSymbol.split("_")[0].toUpperCase();
        }
        return langCode;
    }

    var flag = 0;
    window.addEventListener('click', function (event) {
        var dropDownElem = document.getElementsByClassName("lang-dropdown")[0];
        var langDropDownMenu = document.getElementById("languageDropdownMenu");
        if (event.target != dropDownElem
            && event.target.parentNode != dropDownElem
            && flag % 2 != 0) {
            if (langDropDownMenu.classList.contains("show")) {
                langDropDownMenu.classList.remove("show");
            }
        }
        flag += 1;
    })

    setLanguageCode();
</script>
