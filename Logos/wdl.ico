<script>
jQuery(function($){
    function reset() {
        $('.tabs-to-hamburger').removeClass('active disabled');
        $('.tabs-to-hamburger-button').removeClass('tabs-to-hamburger-button-active').html('<span>Menu </span><i class="icon icon-angle-down"></i>').attr('aria-expanded', 'false');
    }
    function closeNav() {
        $('.tabs-to-hamburger').removeClass('active').addClass('disabled');
        $('.tabs-to-hamburger-button').removeClass('tabs-to-hamburger-button-active').html('<span>Menu </span><i class="icon icon-angle-down"></i>').attr('aria-expanded', 'false');
    }
    function openNav() {
        $('.tabs-to-hamburger').addClass('active').removeClass('disabled');
        $('.tabs-to-hamburger-button').addClass('tabs-to-hamburger-button-active').html('<span>Menu </span><i class="icon icon-angle-up"></i>').attr('aria-expanded', 'true');
    }
    $('.tabs-to-hamburger-button').on('click', function() {
        if ($('.tabs-to-hamburger').is(':hidden')) {
            openNav();
            $('.tabs-to-hamburger').attr('aria-hidden', 'false');
        } else {
            closeNav();
            $('.tabs-to-hamburger').attr('aria-hidden', 'true');
        }
    });
    $('html').on('click touchstart keydown keyup',function(event) {
        reset();
    });
    $('.tabs-to-hamburger-button, .tabs-to-hamburger').on('click touchstart keydown keyup',function(event) {
        event.stopPropagation();
        // allow escape key to close for accessibility
        if (event.which == 27) {
            reset();
            $('.tabs-to-hamburger-button').focus();
        }
    });
    // close if window is resized
    $(window).on('resize', function(){
        reset();
    });
});
</script>


