<script type="text/javascript">
	// Formulario cotizacion
	function getParameterByName(name){
		name=name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
		var regex=new RegExp("[\\?&]"+name+"=([^&#]*)"),
			results=regex.exec(location.search);
		return results===null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	}
	var prodId=getParameterByName('formulario');
	if(prodId=="servidor_dedicado"){
		$("#server_dedicado").css("display", "block");
	}else if(prodId=="servidor_unidad"){
		$("#col_unidad").css("display", "block");
	}
	else if(prodId=="servidor_jaula"){
		$("#col_gabinete").css("display", "block");
	}
	$(function(){
		$('#defaultReal').realperson();
		$.fn.cambia_espacio=function(id){
			switch(id){
				case 'Jaula':
					$('#pregunta_gabinete').show();
					break;
				default:
					$('#pregunta_gabinete').hide();
					break;
			}
		}
	});
</script>
