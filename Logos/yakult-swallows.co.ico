<style>
.floating-banner {
    position: fixed;/* 追従 */
    z-index: 99999;/* 他の要素の下に隠れないように */
    bottom: -10px;/* バナーの上下の位置 */
    right: 0;/* バナーの左右の位置 */
    width: 350px;
}
.floating-banner .csBanner2896 img{
  animation: wobbling_x 0.5s ease-in-out infinite alternate,
  wobbling_y 3.0s ease-in-out infinite alternate;
}
@keyframes wobbling_x {
  0% {
    margin-left: 5px;
  }

  100% {
    margin-left: 0px;
  }
}

@keyframes wobbling_y {
  0% {
    margin-bottom: 0px;
  }

  100% {
    margin-bottom: 2px;
  }
}

.floating-banner .pc {
    width: 100%;/* バナーの横幅を指定 */
}
.floating-banner:hover {
    opacity: 1;/* ホバーで少し透過 */
}
.floating-banner:hover a:hover{
  opacity: 1;/* ホバーで少し透過 */
}
.floating-banner .close{
  display: block;
  position: fixed;
  bottom: 230px;
  right: 1rem;
  color: #fff;
  z-index: 999999;
  text-shadow: 1px 1px 2px #808080;
}
.floating-banner .close:hover{
  opacity: 0.8;
}
@media screen and (max-width: 560px) {/* スマホ用のブレイクポイントを指定 */
    .floating-banner {
      bottom: 0;
      width: 100vw;
    }
    .floating-banner .sp {
      width: 100vw;/* スマホの画面幅いっぱいにバナーを表示 */
    }
    .floating-banner .csBanner2896 img{
      animation: none;
    }
    .floating-banner .close{
      right: 0.5rem;
      bottom: 37vw;
      text-shadow: 1px 1px 2px #808080;
    }
    .floating-banner .close i{
      font-size: 1.7rem;
    }

}

</style>










