<script>
$(function() {
	/* ダイレクトリンクの初期値 */
	initDirectLink();
});
	// 種別コードを保管
	var shuCdList = new Array();

	/* Cookieの取得 */
	function getCookie(c_name) {
		var st = "";
		var ed = "";
		if (document.cookie.length > 0) {
			// クッキーの値を取り出す
			st = document.cookie.lastIndexOf(c_name + "=");
			if (st != -1) {
				st = st + c_name.length + 1;
				ed = document.cookie.indexOf(";", st);
				if (ed == -1)
					ed = document.cookie.length;
				// 値をデコードして返す
				return unescape(document.cookie.substring(st, ed));
			}
		}
		return "";
	}

	/* ダイレクトリンク押下時の処理 */
	function onDirectLink() {
		// 選択チェック
		var errMsg = '';
		if ($('#sel_pmc_dl').val() == '') {
			errMsg = 'ホール名を選択してください。\n';
		}
		if ($('#sel_mdc_dl').val() == '') {
			errMsg += '機種名を選択してください。\n';
		}
		if (errMsg != '') {
			alert(errMsg);
			return;
		}

		// Cookieに選択中の値を保存
		var deferred = setCookie();

		deferred.done(function(){

			// 種別により遷移先を変更
			if (shuCdList[$('#sel_mdc_dl').val()] == '01') {
				document.HallDedamaActionForm.kindcode.value = '01';
			} else if (shuCdList[$('#sel_mdc_dl').val()] == '03') {
				document.HallDedamaActionForm.kindcode.value = '03';
			}
			
				document.HallDedamaActionForm.forward.value = "LIST";
			

			document.HallDedamaActionForm.submit();
		});

	}

	/* Cookieに選択中の値を保存 */
	function setCookie() {

		var deferred = new $.Deferred();

		$.ajax({
	        url : 'CommonSetCookie.do?key=pmc_dl&key=mdc_dl&value='
	        		+ $('#sel_pmc_dl').val() + '&value=' + $('#sel_mdc_dl').val(),
	        type : 'GET',
	        async : true,
	        timeout : '10000',
	        error : function(xhr, status, e) {
        		console.log(xhr, status, e);
	        }
		}).always(function(){
			deferred.resolve();
		});

		return deferred;

	}

	/* ダイレクトリンクの初期選択 */
	function initDirectLink() {
		var pmcDl = getCookie('pmc_dl');
		var mdcDl = getCookie('mdc_dl');
		if (pmcDl && mdcDl && $('#sel_pmc_dl') && $('#sel_mdc_dl')) {
			if ($("#sel_pmc_dl option[value=" + pmcDl + "]").size()) {
				$('#sel_pmc_dl').val(pmcDl);
			}
			if ($("#sel_mdc_dl option[value=" + mdcDl + "]").size()) {
				$('#sel_mdc_dl').val(mdcDl);
			}
		}
	}

	// マイホール
	var subWinName = "myhall";
	var subWinParam = "width=360,height=450,menubar=no,location=no,toolbar=no,resizable=yes";
	var w_myhall = null;
	function myhall_click(phmembCd) {
		  w_myhall = window.open("MypageMyHallConfirm.do?hallcode=" + phmembCd,subWinName, subWinParam);
		  w_myhall.focus();
		}
	//処理後の表示切替
	function myhall_after_kaijyo_sidebar(phmembCd) {
		var delElement = document.getElementById('myhall_' + phmembCd);
		delElement.parentNode.removeChild(delElement);

		var hlListchild = document.getElementById('myhallList').childNodes;
		var childFlg = 0;
		for (var i = 0, len = hlListchild.length; i < len; i++) {
		    if (hlListchild[i].nodeName == 'LI'){
		    	childFlg = 1;
		       break;
		    }
		}
		if (childFlg == 0) {
			var addElement = document.getElementById('emptyMyhall');
			addElement = document.createElement('li');
			addElement.id = 'emptyMyhall';
			addElement.innerHTML = '現在登録されているマイホールはありません。';
			document.getElementById('myhallList').appendChild(addElement);
		}
	}
	// マイホール登録/解除完了画面からの呼び出し（別ウィンドウから）
	function myhall_after_toroku_sidebar(phmembCd, hallName, encryptPmc) {
		var delElement = document.getElementById("emptyMyhall");
		if (delElement != null) {
			delElement.parentNode.removeChild(delElement);
		}

		var addElement = document.createElement('li');
		addElement.id = 'myhall_' + phmembCd;
		addElement.innerHTML = createHtmlForMyHall(phmembCd, hallName, encryptPmc);
		addElement.className = "hlname";
		document.getElementById("myhallList").appendChild(addElement);
	}
	// マイホール用の要素を作成する
	function createHtmlForMyHall(phmembCd, hallName, encryptPmc) {
		var ret = '';
		
		ret = ret + '<div style="float:left;padding: 4px 0"><a href="HallSelectLink.do?hallcode=' + phmembCd + '">';
		ret = ret + hallName +  '</a></div>';
		ret = ret + '<div style="clear:both;"></div>';
		return ret;
	}
	// 画面遷移時、開いたままの場合は閉じる
	onunload = function () {
		if (w_myhall != null && w_myhall.closed == false) {
			w_myhall.close();
		}
	};

	// SSL
	function doSsl(url) {
		var loc = window.location.href;
		var index = loc.lastIndexOf("/");
		var sslUrl = (loc.slice(0,index) + "/" + url).replace("http://", "https://");
		location.href = sslUrl;
	}

</script>
