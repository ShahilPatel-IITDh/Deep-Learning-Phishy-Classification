  <script >
    fetchFinanceDataNav = async () => {
      await fetch('https://airfranceklm.symex.be/stockmarket/home')
        .catch(err => { throw new Error(err.message) })
        .then((response) => { return response.json(); })
        .then((data) => { appendDataNav(data); });
    };

    function appendDataNav(data) {
      document.getElementById("nav-stock-pourcentage").innerHTML = data.value + '€';
      document.getElementById("nav-mobile-stock-pourcentage").innerHTML = data.value + '€';
      var pourcentageIcon = document.getElementById("nav-stock-icon");
      var pourcentageIconMobile = document.getElementById("nav-mobile-stock-icon");
      pourcentageIcon.innerHTML = '&#8623;';
      pourcentageIconMobile.innerHTML = '&#8623;';
      if (data.value_percent >= 0) {
        pourcentageIcon.classList.remove('stock-down-icon-nav');
        pourcentageIconMobile.classList.remove('stock-down-icon-nav');
      } else {
        pourcentageIcon.classList.add('stock-down-icon-nav');
        pourcentageIconMobile.classList.add('stock-down-icon-nav');
      }
    };
    fetchFinanceDataNav();
    setInterval(fetchFinanceDataNav, 20000);
  </script>

