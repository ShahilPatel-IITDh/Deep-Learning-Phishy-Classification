      <script>
    //Stock Price
      fetchFinanceData = async () => {
        await fetch('https://airfranceklm.symex.be/stockmarket/home')
          .catch(err => { throw new Error(err.message) })
          .then((response) => { return response.json(); })
          .then((data) => { appendData(data); });
      };

      function appendData(data) {
        document.getElementById("stock-price").innerHTML = data.value;
        var pourcentage = document.getElementById("stock-pourcentage");
        pourcentage.innerHTML = `${data.value_percent >= 0 ? '+' : ''} ${data.value_percent} %`;
        var pourcentageIcon = document.getElementById("stock-pourcentage-icon");
        pourcentageIcon.innerHTML = '&#8623;';
        if (data.value_percent >= 0) {
          pourcentageIcon.classList.remove('stock-down', 'stock-down-icon');
          pourcentage.classList.remove('stock-down');
          pourcentageIcon.classList.add('stock-up');
          pourcentage.classList.add('stock-up');
        } else {
          pourcentageIcon.classList.remove('stock-up');
          pourcentage.classList.remove('stock-up');
          pourcentageIcon.classList.add('stock-down', 'stock-down-icon');
          pourcentage.classList.add('stock-down');
        }
      };
      fetchFinanceData();
      setInterval(fetchFinanceData, 20000);
    </script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@^3"></script>
    <script src="https://cdn.jsdelivr.net/npm/luxon@^2"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-adapter-luxon@^1"></script>
    <script>
      let stock;
      let mode = "MONTH";

      fetchChartData = async () => {
        let url;
        let timeConfig;

        switch (mode) {
          case "DAY":
            url = 'https://airfranceklm.symex.be/stockmarket/history/1M';
            timeConfig = {
                unit: 'day',
                displayFormats: {
                  quarter: 'ddd'
              }
            };
            break;
          case "MONTH":
            url = 'https://airfranceklm.symex.be/stockmarket/history/4M';
            timeConfig = {
                unit: 'month',
                displayFormats: {
                  quarter: 'MMM'
              }
            };
            break;
          case "YEAR":
            url = 'https://airfranceklm.symex.be/stockmarket/history/24M';
            timeConfig = {
                unit: 'month',
                displayFormats: {
                  quarter: 'YYYY'
              }
            };
            break;
          default:
            url = 'https://airfranceklm.symex.be/stockmarket/history/4M';
            break;
        }

        await fetch(url)
          .catch(err => { throw new Error(err.message) })
          .then((response) => { return response.json(); })
          .then((stockData) => {
            let lang = navigator.language;
            stockData.reverse(); // reorder
            stockData.forEach(function(part, index, stockData) {
              stockData[index]["month"] = luxon.DateTime.fromISO(part.date).setLocale('fr').toFormat('LLL');
            }); // try to inject month data for the chart
            let data = {
              datasets: [
                {
                  label: 'Air France-KLM',
                  data: stockData,
                  fill: false,
                  borderColor: '#21d59b'
                }
              ]
            };
            const config = {
              type: 'line',
              data: data,
              options: { scales: {
                    x: {
                        type: 'time',
                        time: timeConfig,
                        ticks: {
                          color: '#7e84a3'
                        }
                    },
                    y: {
                        ticks: {
                        color: '#7e84a3'
                        }
                    }
                }, animation: false, responsive: true, plugins: { legend: { display: false }, tooltip: {
              enabled: false // <-- this option disables tooltips
            } }, parsing : {
                    xAxisKey: 'date',
                    yAxisKey: 'end'
                  }, elements: {
                          point:{
                              radius: 0
                          }
                      }}
            };
            if(stock) {
              stock.destroy();
            }
            stock = new Chart(document.getElementById("myChart"), config);
            let latest = stockData.pop();
            let value = latest.end;
            value = value.toString().replace(".", ",");
            document.getElementById("stock-price").innerHTML = value + "€";
          });
      };
      fetchChartData();
      setInterval(fetchChartData, 20000);

      function switchMode(newMode) {
        mode = newMode;

        fetchChartData();
      }
    </script>
