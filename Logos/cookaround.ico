    me.isInViewport = function(element, deltatop) {
        var rect = element.getBoundingClientRect();
        var html = document.documentElement;

        //console.debug('[lazyAdv] '+(rect.top - deltatop));
        //console.debug('[lazyAdv] '+window.innerHeight);

        if(rect.top  >= 0 && rect.bottom - deltatop <= (window.innerHeight || html.clientHeight)) {
          console.debug('[lazyAdv] In viewport '+(rect.bottom - deltatop));
          return true;
        }

        if($(window).scrollTop() >= me.minScrollTop) {
          console.debug('[lazyAdv] Init by scrollTop '+$(window).scrollTop());
          return true;
        }
        return false;

