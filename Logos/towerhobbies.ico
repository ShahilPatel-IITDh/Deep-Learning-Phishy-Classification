<iframe id="insync-iframe" referrerpolicy="origin"
       sandbox="allow-scripts allow-modals allow-popups allow-same-origin"
       src="https://dbrd449anfbv4.cloudfront.net/insync_iframe_webchat_js_prod.html"
       style="width:0px;height:0px;background:transparent;position: fixed !important;margin:0;right:0;bottom:0;border:none;overflow:hidden;display:block;z-index:1000;border-radius:0px;box-shadow:none;">Your
       browser does not support iframes.</iframe>
   <script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
   <script type="text/javascript">
        function uuidv4() {
            return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
                var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        }
		function getCookie(c_name) {
           var i, x, y, ARRcookies = document.cookie.split(";");
             for (i = 0; i < ARRcookies.length; i++) {
                 x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
                 y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
                 x = x.replace(/^\s+|\s+$/g, "");
                 if (x == c_name) {
                    return unescape(y);
                }
            }
        }

        function setCookie(c_name, value, exdays) {
          var exdate = new Date();
          exdate.setDate(exdate.getDate() + exdays);
          var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
          document.cookie = c_name + "=" + c_value;
        }
		var chatID = getCookie('chat_ID');
		if (typeof chatID == 'undefined') {
		    setCookie('chat_ID' ,  uuidv4() , 2);
			chatID = getCookie('chat_ID');
		}        // This (userId) should be dynamically assigned as unique user id which remains same across page refresh for a given user
       let userId = chatID;


       ////////////// CHATBOT CONFIG //////////////




       let chatbotConfig = {
           userId: chatID,
           mobile: false,
           market: "us_towerhobbies",
           env: "production",
           appId: "1995e06deeb5e01b61b815207883455f", // For development
           source: "web",
           triggerPoint: "TowerHobbiesWebsite", //Your page reference name
           hostedPageRef: window.location.href,
           contextParams: {
               isIframe: true,
               iframeParentWidth: window.innerWidth,
               iframeParentHeight: window.innerHeight
           }
       }




       //////////////////////////////////////////


       var iframeEl = document.getElementById('insync-iframe');


       let openChat = function (type) {
           iframeEl.style.height = chatbotConfig.styleGuide.screenHeight.toString() + "px";
           iframeEl.style.width = chatbotConfig.styleGuide.screenWidth.toString() + "px";
           iframeEl.style.right = chatbotConfig.styleGuide.right.toString() + "px";
           iframeEl.style.bottom = chatbotConfig.styleGuide.bottom.toString() + "px";

           // Make sure you are sending a string, and to stringify JSON
           iframeEl.contentWindow.postMessage({ type, userId }, '*');
       };




       ////////////// Receive Messages from iFrame //////////////
       window.addEventListener("message", receiveMessage, false);




       function receiveMessage(event) {
           //console.log("event.origin: "+event.origin)
           //Validating domain
           if (event.origin !== "https://dbrd449anfbv4.cloudfront.net")
               return;
           if (event.data.type == 'CloseChat') {
               iframeEl.style.height = "0px";
               iframeEl.style.width = "0px";
           }
           if (event.data.type == 'ShowChatBtn') {
               let button = document.getElementById('insync-chat-button');
               if (typeof chatbotConfig.styleGuide == 'undefined') chatbotConfig.styleGuide = {};
               chatbotConfig.styleGuide = event.data;
               button.style.display = "block";
           }
           if (event.data.type == "StyleIframe") {
               iframeEl.style.borderRadius = chatbotConfig.styleGuide.chatBoxBorderRadius.toString() + "px";
               iframeEl.style.boxShadow = chatbotConfig.styleGuide.chatBoxShadow;
           }
       }

       function sendChatbotConfigToIframe() {
           let type = 'ChatbotConfig'
           iframeEl.contentWindow.postMessage({ type, chatbotConfig }, '*');
       }
       window.setTimeout(sendChatbotConfigToIframe, 3000);


   </script>

