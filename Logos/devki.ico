                success: function (data){
                    $('.img-'+id).attr('src', data);
                    $('.img-'+id).siblings('source').attr('srcset' , data);
                    $('#single_image').attr('href', data);

                    position++;
                    if (position == count){
                        $(object).removeClass('d-block');
