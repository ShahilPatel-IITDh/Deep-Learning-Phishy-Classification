                    }
                    var positionPrev = position - 1;
                    $(object).siblings(".prev-img-btn").attr('data-position' , positionPrev);
                    $(object).siblings(".prev-img-btn").addClass('d-block');
                    $(object).attr('data-position' , position)

                },
            })
        }
    }

