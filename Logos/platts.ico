                $('.homeMobileTypeFilter').closest("ul").find("input[type=checkbox]").first().prop("checked", true);
            }
            var filterUrl = protocol + "//" + host + path + "/" + culture + "/Home/HomeMutipleFilterResult";
            $.ajax({
                type: "post",
                url: filterUrl,
                dataType: "html",
                data: {
                    commodities: commodityFilter == "" ? "all" : commodityFilter,
                    contentTypes: contentsubTypesFilter == "" ? "all" : contentsubTypesFilter,
                    rows: row,
                    houseAdRows: houseAdRow
                },
                success: function (data, status, xhr) {
                    $('#homeNewsResult').empty();
                    $('#homeNewsResult').html(data);
                    $("#loadMoreHome").attr("filterKey", "mobile");
                    if (parseInt(xhr.getResponseHeader("TotalHomeResult")) <= (row - houseAdRow)) {
                        $("#loadMoreHome").hide();
                        $("#backToTop").show();
                    }
                    else {
                        $("#loadMoreHome").show();
                        $("#backToTop").hide();
                    }
                    loadGtmInPartialView();
                },
                error: function (jqXHR, exception) {
                }

            });
            return false;
        }

        $(".filter_toggle_button").on("click", function () {
            if ($(".table-filter-content").css('display') == 'none') {
                $(".filter_toggle_button").removeClass("active");
            }
            else {
