        function GetArticleResult(menu) {

            var lang = location.pathname;
            lang = path != "" ? lang.split('/')[2] : lang.split('/')[1];
            lang = (typeof (lang) != "undefined" && lang != "") ? lang : "";

            var commodity = getCommodity();
            var searchUrl = protocol + "//" + host + path + "/" + lang + "/Home/SearchDetails/";
            $.ajax({
                type: "Post",
                dataType: "html",
                data: {
                    menu: menu, commodity: commodity
                },
                url: searchUrl,
                success: function (response) {
                    switch (menu) {
                        case "Commodity":
                            $("#latestCommodities").empty();
                            $("#latestCommodities").html(response);
                            break;
                        case "Methodology":
                            $("#latestMethodology").empty();
                            $("#latestMethodology").html(response);
                            break;
                        case "ProductsServices":
                            $("#latestINeed").empty();
                            $("#latestINeed").html(response);
                            break;
                        case "MarketInsights":
                            $("#latestMarketInsights").empty();
                            $("#latestMarketInsights").html(response);
                            break;
                        case "Events":
                            $("#latestEvents").empty();
                            $("#latestEvents").html(response);
                            break;
                    }
                },
                failure: function (response) {
                    console.log(response.responseText);
                },
                error: function (response) {
                    console.log(response.responseText);
                }
            });
        }

        function getCommodity() {
            var commodities = ["oil", "lng", "natural-gas", "electric-power",
                "coal", "shipping", "metals", "petrochemicals", "agriculture"];
            var pathname = window.location.pathname;
            var menu = path != "" ? pathname.split('/')[3] : pathname.split('/')[2];
            var commodity = "";
            switch (menu) {

                case "commodities":
                case "product-services":
                    commodity = path != "" ? pathname.split('/')[4] : pathname.split('/')[3];
                    break;
                case "market-insights":
                case "our-methdology":
                    commodity = path != "" ? pathname.split('/')[5] : pathname.split('/')[4];
                    break;
            }
            commodity = commodities.indexOf(commodity) != -1 ? commodity : "";
            return commodity;
        }

    });

</script>





