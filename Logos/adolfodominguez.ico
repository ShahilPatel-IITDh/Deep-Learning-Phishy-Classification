<script type="text/javascript">
    function setLanguages() {
        var euCountries = ['at', 'be', 'bg', 'cz', 'dk', 'ee', 'fi', 'de', 'gr',
            'hu', 'ie', 'it', 'lv', 'lt', 'lu', 'nl', 'pl', 'ro', 'si','sk', 'se'];
        var jpLangs = document.getElementById('jp-lang-options').innerHTML;
        var gbLangs = document.getElementById('gb-lang-options').innerHTML;
        var frLangs = document.getElementById('fr-lang-options').innerHTML;
        var ptLangs = document.getElementById('pt-lang-options').innerHTML;
        var euLangs = document.getElementById('eu-lang-options').innerHTML;
        var defaultLangs = document.getElementById('en-es-lang-options').innerHTML;
        var externalLangs = document.getElementById('external-lang-options').innerHTML;
        var langSelect = document.getElementById('lang-selector-options');
        if (document.getElementById('countrySelector').value === 'jp') {
            langSelect.innerHTML = jpLangs;
        } else if (document.getElementById('countrySelector').value === 'fr') {
            langSelect.innerHTML = frLangs;
        } else if (document.getElementById('countrySelector').value === 'pt') {
            langSelect.innerHTML = ptLangs;
        } else if (document.getElementById('countrySelector').value === 'gb') {
            langSelect.innerHTML = gbLangs;
        } else if (euCountries.indexOf(document.getElementById('countrySelector').value) !== -1) {
            langSelect.innerHTML = euLangs;
        }  else if ((document.getElementById('countrySelector').value === 'au') || (document.getElementById('countrySelector').value === 'nz') ){
            langSelect.innerHTML = externalLangs;
        } else {
            langSelect.innerHTML = defaultLangs;
        }
    }
</script>
