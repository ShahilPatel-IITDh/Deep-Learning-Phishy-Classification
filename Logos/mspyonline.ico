				var arrTags = [], ga = getCookie('_ga'), sequence = getCookie('sequence');
				if (ga !== 'undefined') {
					let preparedGa = 'sh_' + ga.slice(6).split('.').join('-');
					arrTags.push(preparedGa);
				}
				if (sequence !== 'undefined') {
					arrTags.push('s_' + sequence);
				}
				return arrTags;
			}
		}, chatInitTimeout)
	});
