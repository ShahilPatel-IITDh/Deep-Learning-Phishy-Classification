var songPlayer = function(elm_song) {

		var aud_song = document.getElementById(elm_song);

		var isPlaying_song = false;

		var control_song = $("#song_li");

		var control_i_song = $("#song_li i");
		
		var control_li_song = $("#down_song");

		this.play = function(){

			if (isPlaying_song) { aud_song.pause(); isPlaying_song=false; control_song.removeClass("playing"); control_i_song.removeClass("fa-pause"); control_li_song.removeClass("display-block");}

			else { aud_song.play(); isPlaying_song=true; control_song.addClass("playing"); control_i_song.addClass("fa-pause"); control_li_song.addClass("display-block");}

		}

	};
	

var song = new songPlayer("songAudio");

$("#song_li a").click(function(){song.play();});	
	
	
