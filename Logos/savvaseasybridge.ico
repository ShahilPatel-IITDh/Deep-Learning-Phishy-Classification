<script type="text/javascript">
function init() {
    var firstForm = document.forms[0];
    
    if (firstForm) {
        var formElements = firstForm.getElementsByTagName("input");
        
        for (var i = 0; i < formElements.length; i++) {
            var e = formElements[i];
            
            if (
                (! e.disabled) &&
                (e.type.toLowerCase() != 'hidden')
            ) {
                e.focus();
                e.select();
                
                break;
            }
        }
    }
}
</script>


