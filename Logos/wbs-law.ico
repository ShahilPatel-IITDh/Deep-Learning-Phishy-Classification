/* OPTIMIZED DATA LEAK CHECKER FORM */
.gform_wrapper.data-leak-checker { box-shadow: none; border-radius: 0; padding: 0; background: transparent; border: none; }
.gform_wrapper.data-leak-checker #data-leak { background: #ff3c1f; color: #fff; padding: 2rem 1rem !important; border-radius: 0.5rem; text-align: center; box-shadow: 0 1rem 3rem rgb(0 0 0 / 15%); margin-bottom: 3rem !important; }
.gform_wrapper.data-leak-checker #data-leak .form-group { max-width: 32rem; margin: 0 auto; }
.gform_wrapper.data-leak-checker #data-leak .form-label { font-size: 1.5rem; }
.gform_wrapper.data-leak-checker #data-leak .form-label .gfield_required { color: rgba(255 255 255 / 50%); }
.gform_wrapper.data-leak-checker #data-leak p { margin: 0; }
.gform_wrapper.data-leak-checker #data-leak p.hint { margin: 0 0 2rem 0 !important; }
.gform_wrapper.data-leak-checker #data-leak p small { margin-top: 1rem; display: block; color: rgba(255 255 255 / 75%) !important; }
.gform_wrapper.data-leak-checker #data-leak .form-control { font-size: 1.5rem; margin: 1.5rem 0; text-align: center; border-radius: 2rem; border: none; box-shadow: 0 0.5rem 1.5rem rgb(0 0 0 / 25%); }
.gform_wrapper.data-leak-checker #data-leak .btn { font-size: 1.25rem; padding: 0.75rem 4rem; border-radius: 3rem; border: 1px solid #fff !important; }
.gform_wrapper .gform_fields .gfield--width-half {
    width: 50%;
}

