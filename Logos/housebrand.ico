<script>
    var getCookie = function (cookie) {
            var name = cookie + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        },
        setCookie = function (cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        },
        code = {
            "bg": "/bg/bg/",
            "cs": "/cz/cz/",
            "cz": "/cz/cz/",
            "de": "/de/de/",
            "ee": "/ee/et/",
            "es": "/es/es/",
            "et": "/ee/et/",
            "lt": "/lt/lt/",
            "lv": "/lv/lv/",
            "hu": "/hu/hu/",
            "pl": "/pl/pl/",
            "ro": "/ro/ro/",
            "si": "/si/sl/",
            "sk": "/sk/sk/",
            "sr": "/rs/sr/",
            "hr": "/hr/hr/",
            "it": "/it/it/",
            "gb": "/gb/en/",
            "uk": "/ua/uk/",
            "gr": "/gr/el/"
        },
        cookieName = 'Lang',
        dlang = (function (c) {
            if (c && c.length == 7) {
                var lang = c.substr(1, 2);
                if (code[lang] != "undefined") {
                    return lang;
                }
            }
            return navigator.language.substr(0, 2);
        })(getCookie(cookieName)),
        active = document.querySelector("a[data-lang='" + code[dlang] + "']");
    if (active && dlang !== "ho") {
        active.className += " active";
    }
    if (document.location.search.indexOf('backurl') > 0) {
        var backurl = /backurl=(.+)/.exec(document.location.search);
        if (backurl && backurl.length > 0) {
            document.querySelectorAll('a').forEach(function (el) {
                el.setAttribute('href', el.getAttribute('href') + backurl[1]);
            });
        }
    }
    var links = document.getElementsByTagName('a');
    for (var index = 0; index <= links.length - 1; index++) {
        links[index].addEventListener('click', function () {
            setCookie(cookieName, this.getAttribute('data-lang'), 365);
        }, false);
    }
</script>
