    <link rel="alternate" hreflang="x-default" href="https://www.housebrand.com/special/store/">

    <style>
        :root {
            --black: #000;
            --white: #fff;
        }

        html, body, div, span,
        h1, h2, p, a, img, ul, li {
            border: 0;
            margin: 0;
            outline: 0;
            padding: 0;
            vertical-align: baseline
        }

        article, aside, footer, header, main, menu, nav, section {
            display: block
        }

        ul {
            list-style: none
        }

        a {
            text-decoration: none
        }

        html {
            min-height: 100%;
        }

        body {
            min-height: 100%;
            width: 100%;
            background: #FCFCFC url(store_mobile_8-2023.jpg) 82% 58% no-repeat;
            background-size: cover;
            font: 400 15px/1.618 "Helvetica Neue", "Segoe UI", Tahoma, Ubuntu, sans-serif;
            color: #fff;
        }

        main {
            max-width: 86%;
            max-height: 70%;
            overflow: auto;
            min-width: auto;
            position: relative;
            background-color: rgba(252, 252, 252, .8);
            padding: 6% 2.8%;
            margin: 4% auto;
            transform: translateY(0%) translateX(0%);
        }

        .logo {
            max-width: 28%;
            min-width: 140px;
            display: block;
            margin: 0 auto;
            clear: both;
        }

        header {
            font-size: 16px;
            display: block;
            text-align: center;
            padding: 2.4% 2% 3.8%;
            font-weight: 300;
            text-transform: uppercase;
            color: var(--black);
        }

        ul {
            list-style: none;
            column-count: 1;
            -webkit-column-count: 1;
            column-gap: 30px;
            -webkit-column-gap: 30px;
            margin: 0 auto;
        }

        ul li a {
            width: 100%;
            padding: 2px 0 2px 40px;
            display: block;
            box-sizing: border-box;
            color: var(--black);
            line-height: 1.8;
        }

        a.cart, li.cart {
            background: url('koszyk-black.svg') 5px 5px no-repeat;
            color: var(--black);
            background-size: 18px auto;
            transition: all 0.35s ease-out;
        }

        li.cart a {
            display: inline-block;
            width: auto;
            transition: all 0.35s ease-out;
        }

        li.cart a:last-child {
            padding-left: 0;
        }

        li.cart:hover {
            background-position-x: 10px;
        }

        a.active {
            color: #f13131;
        }

        a:hover, a:active, a:focus {
            color: #ff4b4b;
            background-position-x: 10px;
        }

        @media screen and (min-height: 552px) {
            body {
                background-position: 77% top;
            }
        }

        @media screen and (min-width: 640px) {
            body {
                background-size: cover;
            }

            main {
                width: 600px;
                min-width: 320px;
                position: absolute;
                top: 70%;
                left: 0;
                transform: translateY(-50%) translateX(0);
                overflow: hidden;
                padding: 2.4% 2.8% 2.8%;
                margin: 0 auto;
            }

            header {
                font-size: 19px;
            }

            ul {
                -webkit-column-count: 2;
                column-count: 2;
            }

            ul li a {
                line-height: 1.6;
            }
        }

        @media only screen and (orientation: portrait) and (min-height: 540px) {
            main {
                position: absolute;
                top: auto;
                left: 2%;
                right: 2%;
                bottom: 0;
            }
        }

        @media only screen and (min-width: 768px) {
            body {
                background-image: url(store_desktop_8-2023.jpg);
                background-position: center;
            }
        }

    </style>

