<script language="javascript" type="text/javascript">
//<![CDATA[ 
var requestLogin		= false;

function loginInit(){
	try
	{
		requestLogin = new XMLHttpRequest();
	}
	catch (trymicrosoft)
	{
		try
		{
			requestLogin = new ActiveXObject("Msxml2.XMLHTTP");
		}
		catch (othermicrosoft)
		{
			try
			{
				requestLogin = new ActiveXObject("Microsoft.XMLHTTP");
			}
			catch (failed)
			{
				requestLogin = false;
			}  
		}
	}

	if (!requestLogin)
		alert("Error initializing XMLHttpRequest!");
}


function checkFormAccedi()
{
	var f = document.flogin2012;
	var loginDati = true;
	if (f.emailAccedi.value == "")
	{
		alert("Campo E-mail non compilato.");
		f.emailAccedi.focus();
		loginDati = false;
		
		//return false;
	}
	if (f.passwordAccedi.value == "")
	{
		alert("Campo password non compilato.");
		f.passwordAccedi.focus();
		//return false;
		loginDati = false;
	}
	
	if (loginDati == true)
	{
		loginInit();
		
		//attiva il div divAttesaLogin di attesa
		document.getElementById("divAttesaLogin").style.visibility = 'visible';
		
		var col_user	= f.emailAccedi.value;
		var col_pwd		= f.passwordAccedi.value;
		var colsempre	= '';
		var url_ritorno = f.returnUrl.value;
		if (f.ricorda.checked == true)
		{
			colsempre = f.ricorda.value;
		}
		
		var url = "https://www.camperonline.it/_gestioneLogin.asp";
		//NS 20130131 - Sostituito carattere e commerciale
		//var param = "azione=LoginCol&coluser="+ col_user +"&colpwd="+ col_pwd +"&colsempre="+ colsempre;
		col_pwd = col_pwd.replace(/\%/g, "%25");
		col_pwd = col_pwd.replace(/\+/g, "%2b");
		col_pwd = col_pwd.replace(/\$/g, "%24");
		col_pwd = col_pwd.replace(/\&/g, "%26");
		col_pwd = col_pwd.replace(/\,/g, "%2c");
		col_pwd = col_pwd.replace(/\//g, "%2f");
		col_pwd = col_pwd.replace(/\:/g, "%3a");
		col_pwd = col_pwd.replace(/\;/g, "%3b");
		col_pwd = col_pwd.replace(/\?/g, "%3f");
		col_pwd = col_pwd.replace(/\@/g, "%40");
		col_pwd = col_pwd.replace(/\#/g, "%23");
		col_pwd = col_pwd.replace(/\"/g, "%22");
		col_pwd = col_pwd.replace(/\\/g, "%5c");
		col_pwd = col_pwd.replace(/\|/g, "%7c");
		col_pwd = col_pwd.replace(/\{/g, "%7b");
		col_pwd = col_pwd.replace(/\}/g, "%7d");
		col_pwd = col_pwd.replace(/\=/g, "%3d");
		var param = "azione=LoginCol&coluser="+ col_user +"&colpwd="+ col_pwd +"&colsempre="+ colsempre+"&URL_RITORNO="+url_ritorno;
		
		param = param.replace(/ /g, "%20");
		
		requestLogin.open("POST", url, true);
		//VD 20130516 - Tolto per un problema con chrome in fase di login
		requestLogin.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		requestLogin.setRequestHeader("Content-length", param.length);
		
		console.log(param);
		requestLogin.onreadystatechange = loginResponse;
		requestLogin.send(param);
	}
}


function checkLogout(cosa)
{
	loginInit();
	var f = document.flogout2012;
	var url_ritorno = f.returnUrlLog.value;
	var url = "https://www.camperonline.it/_gestioneLogin.asp";
	//NS 20130131 - Sostituito carattere e commerciale
	//var param = "azione=LogoutCol&tmpCosa="+ cosa;
	var param = "azione=LogoutCol&tmpCosa="+ cosa+"&URL_RITORNO="+url_ritorno;
	
	param = param.replace(/ /g, "%20");
			
	requestLogin.open("POST", url, true);
	requestLogin.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	requestLogin.setRequestHeader("Content-length", param.length);
	requestLogin.onreadystatechange = loginResponse;
	requestLogin.send(param);
	
}

function loginResponse()
{
	if (requestLogin.readyState == 4)
	{
		if (requestLogin.status == 200)
		{
			document.getElementById("divAttesaLogin").style.visibility = 'visible';
			
			var responseLogin = requestLogin.responseText;
			var vettore	= responseLogin.split('###');
			document.body.style.cursor = 'default';
			
			//alert("vettore="+ vettore);

			for (var tt=0;tt<vettore.length;tt=tt+3)
			{
				var elem1	= vettore[tt+0];
				var elem2	= vettore[tt+1];
				
				if (elem1 == 'logout')
				{
					switch (elem2)
					{
						case "logout":
							//se il logout su COL ok --> manda al logout dei FORUM
							var f = document.flogout2012;
							
							url_ritorno = vettore[tt+2];
							
							var url_mio = "https://forum.camperonline.it/login_remoto.php?URL_RITORNO="+url_ritorno+"&AZIONE=logout";
							//alert(url_mio);
							window.location.replace(url_mio);							
							document.getElementById('accedili').className = 'off';
							
							break;
						case "ok":
							//utente cancellato correttamente
							var f = document.flogout2012;
							//alert("form submit");
							f.action = "https://forum.camperonline.it/_gestioneLogin.asp?azione=LogoutRemoto";
							f.submit();
							alert("Utente cancellato correttamente");
							
							document.getElementById('accedili').className = 'off'
							
							break;
							
						case "Err":
							//errori cancellazione utente
							alert('Attenzione! Cancellazione utente non riuscita');
							break;
					}
				}
				else
				{
					
					var utente = vettore[tt+2];
					var hash = vettore[tt+3];
					var email = vettore[tt+4];
					//alert("mail="+email);
					var ricorda = vettore[tt+5];
					var url_ritorno = vettore[tt+6];
					switch (elem1)
					{
						case "0":
							//dati login errati
							alert("Attenzione! I dati inseriti non sono corretti. Riprova");
							document.getElementById("divAttesaLogin").style.visibility = 'hidden';
							break;
							
						case "1":

							//login corretto
							//location.reload();
							//document.getElementById("divAttesaLogin").style.visibility = 'hidden';
							//var f = document.flogin2012;
							var url_mio = "https://forum.camperonline.it/login_remoto.php?URL_RITORNO="+url_ritorno+"&AZIONE=login&HASH="+hash+"&UTENTE="+utente+"&EMAIL="+email+"&RICORDA="+ricorda;
							//alert(url_mio);
							window.location.replace(url_mio);
							
							document.getElementById('accedili').className = 'off'
							break;
						
						case "2":
							//registrazione non confermata
							alert("Attenzione! Prima di effettuare il primo accesso devi"+
							" confermare la registrazione.<br />Fai click sul link contenuto"+
							" nell'e-mail di conferma che ti e\' stata inviata");
							document.getElementById("divAttesaLogin").style.visibility = 'hidden';
						break;	
						
						//case default:
							//alert("Attenzione! Si sono verificati errori durante il login. Riprova");
							//break;
					}
				}
			}			
		}
	}	
}

function cambiastilediv(id)
{
	document.getElementById(id).className = 'on';
	if (document.getElementById('accedili').className == 'on')
	{
		document.getElementById('accedili').className = 'off';
	} else {
		document.getElementById('accedili').className = 'on';
	}
}

function goSubmitAccedi(e)
{
	evt = e || event;
	var faiSubmit = false;
	try
	{
		if (evt.keyCode == 13)
			faiSubmit = true;
	}
	catch (err)
	{
	}
	
	if (faiSubmit)
	{
		//document.flogin2012.submit();
		checkForm();
	}	
}
//]]> 
</script> 


