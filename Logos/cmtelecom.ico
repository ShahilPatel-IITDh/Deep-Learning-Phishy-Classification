        }
    ]);

    /**
    * before answer
    * Trigger chatbot events send to CDP
    * Add styling to notify the endUser on the Auto-Translation feature.
    * After onload event trigger the contact options event.
    */
    wcc['965cdb31-0338-4aac-9a28-b55cfd224fe7'].push([
        'on',
        'before:answer',
        function (result) {

            // Trigger chatbot events send to CDP
            if (result.data.outputAdditions && result.data.outputAdditions.triggerEvent) {
                window.dataLayer = window.dataLayer || [];
                window.dataLayer.push({
                    'event': 'chatbotEvent',
                    'content': result.data.outputAdditions.triggerEvent
                });
            }

            if (result.data.answer !== undefined) {
                // Add styling to notify the endUser on the Auto-Translation feature.
                addRealtimeTranslationTag(result.metadata.translation)

