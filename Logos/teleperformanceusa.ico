    <script type="text/javascript">

        $(document).ready(function () {
            if (window.location.href.toLowerCase() == 'https://www.teleperformance.com/en-us/locations/egypt-site/egypt-communication/' ||
                window.location.href.toLowerCase() == 'https://www.teleperformance.com/en-us/locations/egypt-site/egypt-communication') {
                $('.footer-mobile-redirect').hide();
            }
            var userAgent = window.navigator.userAgent;

            if (userAgent.match(/iPad/i) || userAgent.match(/iPhone/i)) {
                $(window).on("scroll", function () { // dispara o evento scroll da janela
                    var AosRowAnimated = 'footer';
                    var Aoswin_heigh = window.innerHeight; // pega a altura da janela
                    var Aoswin_scrol = $(this).scrollTop(); // pega o valor da rolagem da janela
                    var Aosdiv_topo = ($(AosRowAnimated).offset().top - 100); // distância da div até o início do documento
                    var Aosdistanciall = Aosdiv_topo - Aoswin_scrol - Aoswin_heigh; // distância da div até a borda inferior da janela
                    // se a distância da altura da div à borda inferior da janela for menor ou igual a 0
                    if (Aosdistanciall <= -0) {
                        AOS.init({ disable: true });
                    }
                });
            }

            $(window).on("shown.bs.modal", function () {
                AOS.init({ disable: true });
            });

            $(window).on("hidden.bs.modal", function () {
                AOS.init({ disable: false });
            });

            $(".sociallastsource input").val('Contact us - USA - Country page - Website');

            if ('False' == 'True') {
                getUserImageMenu();
            }
        });

        function getUserImageMenu() {
            $.get("/Umbraco/Api/Alumni/GetUser", function (data) {
                appendUserImageMenu(data);
            });
        }

        function appendUserImageMenu(data) {
            if (data != null) {
                if (data.imageProfile != null) {
                    var image = data.imageProfile;
                    $('#button-modal-alumni').html('');
                    $('#button-modal-alumni').append('<img class="alumni-login-button" src="' + image + '" style="border-radius:35px;"/>');

                    if ($('.login-image') != null) {
                        $('.login-image').each(function () {
                            $(this).html('');
                            $(this).append('<img class="alumni-login-button" src="' + image + '" style="border-radius:35px;"/>');
                        });
                    }

                    setTimeout(() => {
                        if ($('.login-image') != null) {
                            $('.login-image').each(function () {
                                $(this).html('');
                                $(this).append('<img class="alumni-login-button" src="' + image + '" style="border-radius:35px;"/>');
                            });
                        }
                    }, 4000);

                }
            }
        }
    </script>

    


    

