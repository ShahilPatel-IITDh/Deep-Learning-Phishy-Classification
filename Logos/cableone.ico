          launcherText: '#fff',
          button: '#8D3B90',
          resultLists: '#8D3B90',
          header: '#8D3B90',
          articleLinks: '#8D3B90'
        },
        helpCenter: {
          suppress: true
        },
        chat: {
          hideWhenOffline: true,
          departments: {
            enabled: [],
            select: 1500003304301
          },
          concierge: {
            name: '',
            title: ''
          },
          prechatForm: {
            greeting: ''
          }
        },
        contactForm: {
          suppress: true
        },
      }
    };

    zE(function () {
      zESettings.webWidget.chat.concierge.name = 'Sparklight';
      zESettings.webWidget.chat.concierge.title = "Sparklight Support";
      zESettings.webWidget.color.theme = { badge: '#8D3B90', primary: '#8D3B90', bubble: '#8D3B90' };
      zESettings.webWidget.chat.prechatForm.greeting = 'Thank you for choosing Sparklight as your service provider, how may I help you today?';

      // Registers a callback to be fired when a chat starts.
      zE('webWidget:on', 'chat:start', () => {
        var eadd = ["", "", "", ""].filter(Boolean).join(", ");

        if ("" != null) {
            var amsg = "Service Address: " + eadd;
            zE('webWidget', 'chat:send', amsg);
          }
      });

      // Registers a callback to be fired when the widget successfully connects to the server.
      zE('webWidget:on', 'chat:connected', () => {
        $zopim(function () {
          $zopim.livechat.setStatus('offline');
        });
        zESettings.webWidget.chat.departments.enabled = [];
        zE('webWidget', 'updateSettings', zESettings);

        // enable testing in sandbox 
        if (document.querySelector(`[data-department="1500002574962"]`) != null) {
          var sandbox_status = zE('webWidget:get', 'chat:department', 1500002574962);
          console.log("sandbox status", sandbox_status);
          if (sandbox_status.status == 'online') {
            $zopim(function () {
              $zopim.livechat.setStatus('online');
            });

            zESettings.webWidget.chat.departments.select = 1500002574962; // doesn't exist?
            zESettings.webWidget.chat.departments.enabled = '';
            zE('webWidget', 'chat:addTags', ["Residential"]);
            zE('webWidget', 'updateSettings', zESettings);

            zE('webWidget', 'open');
          } else {
            zE('webWidget', 'hide');
          }
        } else {
          if (window.location.href.toLowerCase().indexOf("ebb") > -1
          || window.location.href.toLowerCase().indexOf("acp") > -1) {
          var ebb_status = zE('webWidget:get', 'chat:department', 1500003304381);
          console.log(ebb_status);
          if (ebb_status.status == 'online') {
            $zopim(function () {
              $zopim.livechat.setStatus('online');
            });

            zESettings.webWidget.chat.departments.select = 1500003304381;
            zESettings.webWidget.chat.departments.enabled = [];
            zE('webWidget', 'chat:addTags', ["Residential", "EBB", "ACP"]);
            zE('webWidget', 'updateSettings', zESettings);
            zE('webWidget', 'open');

          } else {
            zE('webWidget', 'hide');
          }
        }
        else {
          var isc_status = 	zE('webWidget:get', 'chat:department', 1500003304301);
          console.log(isc_status);
          if (isc_status.status == 'online') {
            $zopim(function () {
              $zopim.livechat.setStatus('online');
            });
            zESettings.webWidget.chat.departments.select = 1500003304301;
            zESettings.webWidget.chat.departments.enabled = [];
            zE('webWidget', 'chat:addTags', ["Residential", "ProductPage"]);
            zE('webWidget', 'updateSettings', zESettings);

            var chat_band = document.getElementById("chat-band");
            if (chat_band != null || chat_band != undefined) {
              chat_band.classList.remove("d-none");
            }
          } else {
            zE('webWidget', 'hide');
          }
        }
        }
      });

    })

    function chatProLink(dept) {
      zE(function () {
        var department_status = zE('webWidget:get', 'chat:department', dept);
        if (department_status.status == 'online') {
          $zopim.livechat.setStatus('online');
        }
        switch (department_status.id.toString()) {
          case "1500003312402":
            zESettings.webWidget.chat.departments.select = 1500003312402;
            zESettings.webWidget.chat.departments.enabled = [];
            zE('webWidget', 'chat:addTags', ["Residential", "ContactUs"]);
            zE('webWidget', 'updateSettings', zESettings);
            zE('webWidget', 'open');
            break;
          case "1500003312422":
            zESettings.webWidget.chat.departments.select = 1500003312422;
            zESettings.webWidget.chat.departments.enabled = [];
            zE('webWidget', 'chat:addTags', ["PID: No ID", "Not Logged In", "Residential", "ContactUs"]);
            zE('webWidget', 'updateSettings', zESettings);
            zE('webWidget', 'open');
            break;
          case "1500003304301":
            zESettings.webWidget.chat.departments.select = 1500003304301;
            zESettings.webWidget.chat.departments.enabled = [];
            zE('webWidget', 'chat:addTags', ["Residential", "Sales", "ContactUs"]);
            zE('webWidget', 'updateSettings', zESettings);
            zE('webWidget', 'open');
            zE('webWidget', 'show');
            window.adaEmbed.stop();
            break;
          default:
            console.log("No Teams Available.");
        }
      });
    }
    zE('webWidget:on', 'chat:start', function () {
      
      console.log('successfully started a Zendesk Chat!');
    });
  </script>
  <!-- End of contactus Zendesk Widget script -->
</div>
