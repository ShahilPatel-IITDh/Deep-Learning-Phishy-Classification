    <script language=javascript>

        var dispCHiL_timeout = null;
        function dispCHiL(x) {
            if (dispCHiL_timeout) clearTimeout(dispCHiL_timeout);
            var hmidx = 0;
            var ck_cibev = getCookie("TLR_CHiL");
            if (!ck_cibev) {
                ck_cibev = "0,0,0,0";
            }
            var ck_cibevs = ck_cibev.split(',');

            hmidx = parseInt(ck_cibevs[0]);
            if (x && x < 0) {
                hmidx -= 7;
                if (hmidx < 0)
                    hmidx = 0;
            }
            if (hmidx > 60) {
                hmidx = 0;
            }

            var cids = __cids_HL;
            $("#cEv_1").hide();
            $("#cEv_2").hide();
            $("#cEv_3").hide();
            $("#cEv_4").hide();
            $("#cEv_5").hide();
            $("#cEv_6").hide();
            
            $("#cEv_1").html(getCidHLHtml(cids[hmidx % cids.length], 300));
            $("#cEv_2").html(getCidHLHtml(cids[(hmidx + 1) % cids.length], 146));
            $("#cEv_3").html(getCidHLHtml(cids[(hmidx + 2) % cids.length], 146));
            $("#cEv_4").html(getCidHLHtml(cids[(hmidx + 3) % cids.length], 146));
            $("#cEv_5").html(getCidHLHtml(cids[(hmidx + 4) % cids.length], 148));

            $("#cEv_6").html(getCidHLHtml(cids[(hmidx + 5) % cids.length], 146));
           
            $("#cEv_1").fadeIn("slow");
            $("#cEv_2").fadeIn("slow");
            $("#cEv_3").fadeIn("slow");
            $("#cEv_4").fadeIn("slow");
            $("#cEv_5").fadeIn("slow");
            $("#cEv_6").fadeIn("slow");
            ck_cibevs[0] = (hmidx + 6) + "";


            $("#TbCHL img").css("opacity", "0.60");
            $("#TbCHL img").css("filter", "alpha(opacity=60)");
            $("#TbCHL img").attr("onmouseover", "this.style.opacity=1;");
            $("#TbCHL img").attr("onmouseout", "this.style.opacity=0.6;");

            setCookieHr("TLR_CHiL", ck_cibevs.join(","));
            dispCHiL_timeout = setTimeout(dispCHiL, 30 * 1000);
        }
        function getCidHLHtml(car,wd){
            var img=car.img;
            if(wd>320)
                img=img.replace("_1T","_1");
            if(wd>100 && wd<=320)
                img=img.replace("_1T","_1T3");
            return "<div style='width:"+wd+"px;height:"+(wd/4*3)+"px;overflow:hidden;'><a href='../iCar/CarDet.aspx?cid="+car.cid+"'><img alt='รถไฮไลท์' src='"+img+"' style='width:"+wd+"px' "+(wd>300?"onload=adjImgSize(this,"+wd+")":"")+" /></a>";
         }
         function adjImgSize(c,wd){
            var hg=wd/4*3;
            var owd = c.width;
            var ohg = c.height;
            if(ohg<hg && ohg*owd>0){
                c.style.height=hg+"px";
                c.style.width=(hg/ohg*owd)+"px";
            }
        }
        function setCookieHr(c_name, value) {
            var exdate = new Date();
            var time = exdate.getTime();
            time += 5 * 60 * 1000;
            exdate.setTime(time);
            document.cookie = c_name + "=" + escape(value) + ";expires=" + exdate.toGMTString() + ";path=/" + tlr_domain;
        }
         $(function () {
             dispCHiL();
         });
</script>
