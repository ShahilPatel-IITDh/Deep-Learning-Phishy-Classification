    <script type="text/javascript">
        function isSelected(val1, val2) {
            if (val1 == val2)
                return " selected";
            return "";
        }
        function setMake(idx, sM) {
            var c = $('#Make' + idx);
            c.html("<option value='0' " + isSelected(sM, '0') + ">----  เลือกยี่ห้อ  ----</option>");
            var aM = eval("MK.split(',')");
            for (var i = 0; i < aM.length; i += 2) {
                c.append("<option value='" + aM[i] + "' " + isSelected(sM, aM[i] == "0" ? "-1" : aM[i]) + ">" + aM[i + 1] + "</option>");
            }

            setModel(idx, '0');
        }

        function setModel(idx, sM) {
            var c = $('#Model' + idx);
            var mkid = $('#Make' + idx).val();
            c.html("<option value='0' " + isSelected(sM, '0') + ">----  เลือกรุ่น  ----</option>");
            if (mkid == "0") {
                c.prop("disabled", true);
            } else {
                var aM = eval("MD['" + mkid + "'].split(',')");
                for (var i = 0; i < aM.length; i += 2) {
                    c.append("<option value='" + aM[i] + "' " + isSelected(sM, aM[i] == "0" ? "-1" : aM[i]) + ">" + aM[i + 1] + "</option>");
                }
                c.prop("disabled", false);

            }
            setBGo();
        }
        function bPopGo(url) {
            if ($("#ModelB").val() == "0") return;
            var sQ = '?mk=' + $("#MakeB").val();
            sQ += '&md=' + $("#ModelB").val();
            location = url + sQ;
        }
        function setBGo() {
            $("#bBGo").prop("disabled", $("#ModelB").val() == "0");
        }

        function popUpMM(title, url) {
            var t = '';
            t += "<div style='width:250px;height:150px;'><div  style='margin:10px 10px 0 20px;'>" + title + "<table style='margin-top:10px'><tr valign=top><td align=right>";
            t += "<select style='width:130px;font-size:16px' id='MakeB' name='MakeB' onchange=\"setModel('B','0')\">";
            t += "<option value='0'>---- เลือกยี่ห้อ   ----</option>";
            t += "</select>";
            t += "</td>";
            t += "<td rowspan=2>";
            t += "<input type=button id=bBGo onclick=\"bPopGo('" + url + "')\" style='height:50px;width:70px;font-size:16px' value='Go' />";
            t += "</td>";
            t += "</tr>";
            t += "<tr><td>";
            t += "<select style='width:130px;;font-size:16px' id='ModelB' name='ModelB' disabled onchange=\"setBGo()\">";
            t += "<option value='0'>---- เลือกรุ่น  ----</option>";
            t += "</select><br />";
            t += "</td></tr></table></div></div>";

            $.fancybox(t, {
                'width': 300,
                'height': 400,
                'autoScale': false,
                'transitionIn': 'none',
                'transitionOut': 'none',
                'onComplete': function () {
                    setMake("B", '0');
                    setBGo();
                }
            });
        }
    </script>
