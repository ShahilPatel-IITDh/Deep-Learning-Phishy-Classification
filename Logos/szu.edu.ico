            $('body').removeClass('fixedok');
            setTimeout(function() {
                $(window).scrollTop(0);
                $('body').removeClass('fixed');
            }, 300);
        }
        if (!$('.fix-top').hasClass('fixed') && deltaY < 0) {
            $('.fix-top').addClass('fixed');
            $('body').addClass('fixed');
            setTimeout(function() {
                $(window).scrollTop(5);
                $('body').removeClass('fixed');
                
                $(window).scrollTop(0);
            }, 300);            
