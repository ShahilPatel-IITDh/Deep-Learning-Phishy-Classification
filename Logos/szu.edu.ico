<script>  
 // 计时器  
 var timerFun = function() {  
 if ($('.banner').hasClass('s1')) {  
 $('.banner .bannerslick').slick('slickGoTo', 1);  
 $('.banner').removeClass('s1').addClass('s2');  
 } else if ($('.banner').hasClass('s2')) {  
 $('.banner .bannerslick').slick('slickGoTo', 2);  
 $('.banner').removeClass('s2').addClass('s3');  
 } else {  
 $('.banner .bannerslick').slick('slickGoTo', 0);  
 $('.banner').removeClass('s3').addClass('s1');  
 }  
 }  
 var timer = setInterval(timerFun, 25000);  
 $('.bannerslick').slick({  
 dots: false,  
 arrows: false,  
 slidesToShow: 1,  
 slidesToScroll: 1,  
 autoplay: false,  
 autoplaySpeed: 4900,  
 });  
 $('.bannerslick').on('beforeChange', function(a, b, index) {  
 $('.banner').removeClass('ss1 ss2 ss3').addClass('ss' + (index + 1));  
 });  
 $('.bannerslick').on('afterChange', function(a, b, index) {  
 clearInterval(timer);  
 timer = null;  
 $('.banner').removeClass('s1 s2 s3').addClass('s' + (index + 1));  
 timer = setInterval(timerFun, 25000);  
 });  
 $('.banner .ani-01').click(function() {  
 $('.banner .bannerslick').slick('slickGoTo', 0);  
 });  
 $('.banner .ani-02').click(function() {  
 $('.banner .bannerslick').slick('slickGoTo', 1);  
 });  
 $('.banner .ani-03').click(function() {  
 $('.banner .bannerslick').slick('slickGoTo', 2);  
 });  
 </script> 
