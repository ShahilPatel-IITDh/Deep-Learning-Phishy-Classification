                            <div class="col-xs-11 col-sm-10 col-md-10 col-lg-10">
                                <div class="sectionTitle he fontBook">{{assetTitle}}</div>
                                <div class="descriptionText text">{{assetDescription}}</div>
                            </div>
                        </div>
                    </div>
                    <div class="toolbar-container">
                        <nav class="toolbar">
                            <ul class="toolbar-list">
                                <li><a title="Download the asset" id="download-link" class="toolbtn download icon-download exclude-ligthbox-handler"></a></li>
                                <li><a title="Share via email" class="toolbtn mail icon-email exclude-ligthbox-handler" data-share-tracking-analytics="SHARE" data-share-channel="mailto"></a></li>
                                <li class="share-wrap"><a class="toolbtn share icon-share"></a>
                                    <ul class="share-dropdown">
                                        {{#each socialItems}}
                                            <li class="fontLight">
                                                <a
                                                    title="{{title}}"
                                                    data-share-channel="{{share-channel}}"
                                                    data-share-tracking-analytics="SHARE"
                                                    target="_blank"
                                                    data-share-url="{{resourceLink}}"
                                                    data-is-share-link="true"
                                                >
                                                    <span class="{{class}}"></span>{{title}}
                                                </a>
                                            </li>
                                        {{/each }}
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                     <div class="search-toc-container">
                        <div class="content-btn_mobile">
                            <span class="icon-menu"></span>
                            <span class="content-label fontBook">Contents</span>
                        </div>
                        <div class="search-form">
                            <form class="search__form" action="#">
                                <div class="search__holder">
                                    <div class="search__field">
                                        <input class="search__input fontBook" type="text" name="searchInput"
                                               placeholder="Search" autocomplete="off">
                                        <span class="search__icon icon-search">
                                            <input type="submit" class="search__btn">
                                        </span>
                                        <span class="clear-search-btn"><span class="icon-close"></span></span>
                                    </div>
                                </div>
                            </form>
                            <div class="matches__holder fontLight">
                                <div class="search-results-control">
                                    <span class="overall__matches"></span>
                                    <span class="find-arrow find__prev icon-arrowleft"></span>
                                    <span class="find-arrow find__next icon-arrowright"></span>
                                </div>
                                <div class="entries"><span class="num__count"></span></div>
                            </div>
                        </div>
                     </div>
                </div>
                <div class="outlinePanel jcf-scrollable" data-jcf='{"handleResize": false}'>
                    <h2 class="hd outline-headline fontRegular">Contents</h2>
                    <ul class="outline-tree fontLight"></ul>
                </div>
                <div class="popupFooter fontLight">
                    <div class="fullscreen-control">Fullscreen<a href="#" class="fullscreen-link"><span
                                class="icon-fullscreen"></span></a></div>
                    <div class="zoom-control"><span class="zoom-control__label">Zoom</span><a href="#" class="zoomOut"><span class="icon-zoomOut"></span></a><a href="#" class="zoomIn"><span class="icon-zoomIn"></span></a></div>
                </div>
            </div>
            <div class="popupContent jcf-scrollable">
                {{{popupContent}}}
            </div>
        </div>
    </div>
</script>
<script class="compatibilityLightbox" type="text/x-handlebars-template">
    <div class="lightboxPopup lightboxCompatibility" id="openPopup">
        <div class="wrapper">
            <div class="header">
                <div class="title he fontBook">{{compatibilityTitle}}</div>
                <div class="toolbtn exit icon-close mfp-close"></div>
            </div>
            <div class="popupContent">
                <div class="compatibility-wrapper">
                    <div class="compatibility-content">
                        <div class="compatibility-headline">
                            {{compatibilityHeadline}}
                        </div>
                        <div class="compatibility-body">
                            {{compatibilityContent}}
                        </div>
                        <div class="compatibility-button-wrapper">
                            <a class="compatibility-button fontRegular button2017 version1">
                                <span class="buttonText">{{compatibilityConfirmButton}}</span>
                                <span class="hoverboxes">
                                    <span class="hoverbox"></span>
                                    <span class="hoverbox"></span>
                                    <span class="hoverbox"></span>
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
<script class="messageLightbox" type="text/x-handlebars-template">
    <div class="popup-header">
        <p class="title">{{headerMessage}}</p>
        <button class="close-button" type="button">×</button>
    </div>
    <div class="popup-container">
        {{> content}}
        <div class="loading-bar-overlay">
            <div class="loadingBar">
                <div class="bar bar-first"></div>
                <div class="bar bar-second"></div>
                <div class="bar bar-third"></div>
            </div>
        </div>
    </div>
</script>
<script class="html-content-lightbox" type="text/x-handlebars-template">
    <div class="popupHeader">
        {{#if isHeaderConfigured}}
        <div class="header-container grid-container">
            <div class="row">
