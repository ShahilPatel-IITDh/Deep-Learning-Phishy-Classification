    var carousel;

    $(document).ready(function() {
        
        carousel = $('.owl-carousel').owlCarousel({items: 1, lazyLoad: true, loop: true, lazyLoadEager: 1,
        });

        carousel.on('changed.owl.carousel', function(e) {
            $('.photo-pager-cover span').text(e.page.index + 1);
        })
            .on('loaded.owl.lazy', function(e) {
                $(e.element).centerImage()
            });

// disable scroll

        carousel.on('drag.owl.carousel', function(event) {
            document.ontouchmove = function (e) {
                e.preventDefault()
            }
        })

// enable scroll
        carousel.on('dragged.owl.carousel', function(event) {
            document.ontouchmove = function (e) {
                return true
            }
        })

        $('.owl-lazy').on('mousedown', function (evt) {
            $('.owl-lazy').on('mouseup mousemove', function handler(evt) {

                if (!carousel.is('dragging') && evt.type === 'mouseup') {
                    let dataAction = $(this).attr('data-action');
                    let dataLink = $(this).attr('data-link');
                    let general = $(this).attr('data-general');
                    let general_title = $(this).attr('data-title');

                    if(general)
                    {
                        $(".priceTableModal .header").html(general_title);
                        $(".priceTableModal .content").html(decodeURIComponent(general.replace(/\+/g, ' ')));
                        $(".priceTableModal").modal({
                            duration: 0
                        }).modal('show');
                    }
                    else if(dataAction && dataAction === 'popUpPriceTable')
                    {
                        $(".priceTableBtn").trigger('click');
                    }
                    else if(dataLink)
                    {
                        window.location = dataLink;
                    }
                }
                $('.owl-lazy').off('mouseup mousemove', handler);
            });
        });

    });

