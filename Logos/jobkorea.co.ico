    function BrazeSetCustomUserAttribute(ATTR_TYPE, PROP_STRING) {
        if (ATTR_TYPE == 5
            || ATTR_TYPE == 100
            || ATTR_TYPE == 104
            || ATTR_TYPE == 105
            || ATTR_TYPE == 106
            || ("" != "" || "" != "")) {

            if (ATTR_TYPE == 5
                || ATTR_TYPE == 100
                || ATTR_TYPE == 104
                || ATTR_TYPE == 105
                || ATTR_TYPE == 106) {
                    appboy.initialize("b9795c74-cdce-4881-a000-eb185c0d072e", {
                        baseUrl: "sdk.iad-05.braze.com",
                        allowUserSuppliedJavascript: true
                    });

                    appboy.display.automaticallyShowNewInAppMessages();
                    appboy.openSession();
            }

            var ATTR_NAME = SetAttributeNameByType(ATTR_TYPE);
            var PROP_STRING = SetAttributePropertyByType(ATTR_TYPE, PROP_STRING);
            var DATA_TYPE = SetAttributeDataType(ATTR_TYPE);

            if (DATA_TYPE == "array") {
                appboy.getUser().addToCustomAttributeArray(
                    ATTR_NAME,
                    PROP_STRING
                );
            }
            else {
                appboy.getUser().setCustomUserAttribute(
                    ATTR_NAME,
                    PROP_STRING
                );
            }
        }
        //alert('[PC] Braze Attribute Call:' + DATA_TYPE+ "-" + ATTR_NAME + "-" + PROP_STRING);
    }

