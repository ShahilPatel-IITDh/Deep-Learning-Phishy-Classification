    rnd = 0; // 19.10.07 NCS 무료 제거로 1개 남음

    if (rnd > 0 && (cmpid != "" && cmpid != null)) {
        setCookieFirstLayer("LayerName", cmpid, 18250);
        // 로그인하지 않고, 쿠키 조건이 맞는 경우
        if ((memId.length < 1) && (mCookie.indexOf("MainLayer=done") < 0)) {
            $('.lyADWrap').show();
            $('.lyAD_' + rnd).show();
        }
    }

