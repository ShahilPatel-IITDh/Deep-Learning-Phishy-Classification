<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.16.0/jquery.validate.min.js"></script>

<script>
  const dropDownContent = document.querySelector(".drop-down-content");
  const contentClasses = dropDownContent.classList;
  const dropDownMenu = document.querySelector(".drop-logo-container");
  const dropDownExit = document.querySelector(".drop-icon");
  const dropDownShow = document.querySelector(".menu-burger");
  const lazyLoader = document.querySelector("section");

  window.onload = function() {
    lazyLoader.classList.add("b-loaded");

  };

  function collapseMenu(){
    if(contentClasses.contains("showContent")){
       contentClasses.remove("showContent");
      	dropDownExit.classList.remove("showMenu");
      	dropDownMenu.classList.remove("showMenu");
    }

  }
  function expandMenu(){
    contentClasses.add("showContent");
    dropDownExit.classList.add("showIcon");
    dropDownMenu.classList.add("showMenu");
  }
  dropDownExit.addEventListener("click", collapseMenu);
  dropDownShow.addEventListener("click", expandMenu);
</script>

<script>
    $(function() {
        // Prevent spaces as prefix.
        $('[type="email"], [type="text"]').on('keypress', function(e) {
            var key =  e.charCode || e.code;
            if(key == 32 && ($.trim(this.value) == '' || this.value.substr(-1) == ' ')) {
                return false;
            }
            return true;
        });

        // Form submit.
        $('#register').validate({
            submitHandler: function(form) {
                var email = $('[name="email"]').val().toLowerCase(),
                        emailDomain = email.split('@')[1],
                        fullName = $.trim($('[name="fullname"]').val()),
                        firstName = $.trim(fullName.substr(0, fullName.indexOf(' '))),
                        lastName = $.trim(fullName.substr(fullName.indexOf(' ') + 1)),
                        rejectUrl = $('[name="rejecturl"]').val().split('?')[0],
                        successUrl = $('[name="successurl"]').val().split('?')[0];

                // Re-check for email domain format.
                if(emailDomain.indexOf('.') < 1) {
                    $('[name="email"]')[0].setCustomValidity('NOPE!');
                    return false;
                }

                // Fill name.
                if(! firstName) {
                    firstName = lastName;
                    lastName = '';
                }
                $('[name="firstname"]').val(firstName);
                $('[name="lastname"]').val(lastName);

                var redirectQs = $.param({
                    clickid: '',
                    email: email,
                    firstname: firstName,
                    lastname: lastName
                });
                $('[name="rejecturl"]').val(rejectUrl + '?status=fail&' + redirectQs);
                $('[name="successurl"]').val(successUrl + '?status=success&' + redirectQs);


                form.submit();
            }
        });

    });
</script>