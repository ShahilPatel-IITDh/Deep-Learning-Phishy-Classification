	<script language="javascript" type="text/javascript">
		function After5Sec() {
			document.getElementById('ERROR').innerHTML = '';
			if (document.getElementById("input_name").value != "") {
				document.index.submit();
			}
		}
		function SetAfter5Sec() {
			setTimeout('After5Sec();', 4000);
		}
		function sleep(waitMsec) {
			var startMsec = new Date();
			while (new Date() - startMsec < waitMsec);
		}
		function CheckPrivacyPolicy() {
			if (document.getElementById("CHECK").checked == true) {
				document.getElementById("CONNECT").disabled = "";
			}
			else {
				document.getElementById("CONNECT").disabled = "disabled";
			}
		}
		function OffPrivacyPolicy() {
			document.getElementById("CHECK").checked = true;
		}
		function AddPrivacyPolicy() {
			document.getElementById("CONNECT").disabled = "disabled";
			document.getElementById("CHECK").checked = false;
		}
		function OnLinkClickPrivacy() {
			document.cookie = 'webaxs_privacy=1.2; max-age=31536000;';   
		}
		function StatusPrivacy() {
			if (document.getElementById("CHECK").checked == false) {
				return false;
			}
			else {
				document.cookie = 'webaxs_privacy=1.2; max-age=31536000;';   
			}
		}
