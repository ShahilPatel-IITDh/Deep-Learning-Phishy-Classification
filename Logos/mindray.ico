    <div id="chooseLocation" class="modal mr-home-modal">
        <div class="mr-home-modal-block">
            <div class="mr-home-modal-title">Choose your location</div>
            <div class="mr-home-modal-subtitle">It looks like you are located in: <strong id="countryName"></strong></div>
            <div class="mr-home-modal-desc">
                You are trying to view a page from a different country or region. *Not all products and services may be available in your country or region.
            </div>
            <div class="mr-home-modal-group">
                <a id="btnGotoCountry" href="javascript:gotoSite(1, 'btnGotoCountry');" data-url="" class="btn btn-more">GO TO <span id="siteName"></span></a>
                <a id="btnGotoGlobal" href="javascript:gotoSite(2, 'btnGotoGlobal');" data-url="/en" class="btn btn-primary-red">CONTINUE TO GLOBAL</a>
            </div>
            <div class="mr-home-modal-group-rb">
                <label class="mr-home-modal-label" for="homeModalRemember">
                    <input id="homeModalRemember" class="mr-home-modal-checkbox" type="checkbox"/> Remember my selection
                </label>
            </div>
            <input type="hidden" id="parentPath" value="/content/dam/xpace/all-sites"/>
            <input type="hidden" id="currentSite" value="CONTINUE TO GLOBAL"/>
        </div>
    </div>

    <script>
        $(function () {
            getSwitchSiteInfo()
        });

        function getSwitchSiteInfo() {
            var currentSite = window.location.href.split('/')[3];
            $.get('/libs/granite/csrf/token.json', function (data, status) {
                let formData = new FormData();
                formData.append('_charset_', 'utf-8');
                formData.append(':cq_csrf_token', data.token);
                formData.append('parentPath', $('#parentPath').val());
                formData.append('currentSite',currentSite);
                $.ajax({
                    type: 'POST',
                    dataType: 'json',
                    url: '/bin/xpace/switchsite',
                    data: formData,
                    processData: false,
                    contentType: false,
                    success: function (data) {
                        //console.log(data);

                        if (data.needSwitch) {
                            $("#countryName").text(data.countryName);
                            $("#siteName").text(data.siteName);
                            $("#btnGotoCountry").attr('data-url', data.siteUrl);
                            initSite();
                        }
                    }
                });
            });
        }

        function initSite() {
            var flag;
            if (navigator.cookieEnabled) {
                for (var c of document.cookie.split(';')) {
                    if (c.split('=')[0].trim() == 'xpace-show-choose-location') {
                        flag = c.split('=')[1];
                    }
                }
            }

            if (flag == "1") {
                $("#btnGotoCountry")[0].click();
            } else if (flag == "2") {
                // nothing to do
            } else {
                $('#chooseLocation').modal();
            }
        }

        function gotoSite(flag, id) {
            if ($('#homeModalRemember').prop('checked') === true) {
                if (navigator.cookieEnabled) {
                    document.cookie = "xpace-show-choose-location=" + flag + ";path=/;";
                }
            }

            $.modal.close();;
            if (flag == 1) {
                window.location.href = $("#" + id).data("url");
            }
        }
    </script>



    
