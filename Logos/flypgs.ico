<script>
    //flybot
    window.addEventListener("load", () => {
        setTimeout(() => {
            $(window).on("scroll", function () {
                if ($('.cbot-animation-holder')) {
                    var scrollTop = $(window).scrollTop();
                    if (scrollTop > 1560 - 500) {
                        $('.cbot-animation-holder').css("visibility", "visible");
                    } else {
                        if (window.screen.width < 640 && document.body.classList.contains("nxm-homepage")) {
                            $('.cbot-animation-holder').css("visibility", "hidden");
                        }
                    }
                }
            });
            $("#flybot-mobil-img").click(function () {
                $(".cbot-dialog-button-regular").trigger("click");
            });
            //observe datepicker to close flybot

            function callback(mutationList, observer) {
                mutationList.forEach(function (mutation) {
                    if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
                        //console.log("mutation.target" , mutation.target);
                        if (mutation.target.classList.contains("true")) 
                            document.querySelector(".cbot-animation-holder")?.classList.add("cbot-animation-holder--none");
                        else
                            document.querySelector(".cbot-animation-holder")?.classList.remove("cbot-animation-holder--none");
                    }
                })
            }

            const options = {
                attributes: true
            }
            const observer = new MutationObserver(callback)
            observer.observe(document.querySelector(".cbot-none"), options)
            
        }, 4000)

    })
    //flybot
</script>
