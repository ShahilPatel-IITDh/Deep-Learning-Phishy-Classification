            document.addEventListener("DOMContentLoaded", () => {
                let searchByBudgetDataLayer = (searchByBudgetEl, eventType = "select_promotion") => {
                    let creative_name = null;
                    if (searchByBudgetEl.classList.contains("searchByBudget-bottom-area")) {
                        creative_name = "HomePage_TripFinder"
