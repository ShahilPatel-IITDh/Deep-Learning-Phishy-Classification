                    dataLayer.push({
                        'event': eventType,
                        'ecommerce': {
                            'items': [{
                                'promotion_id': "TripFinder2",
                                'promotion_name': "TripFinderSearch",
                                'creative_name': creative_name,
                                'creative_slot': "1"
                            }]
                        }
                    })
                }

                const observer = new IntersectionObserver((entries) => {
                    entries.forEach((entry) => {
                        if (entry.isIntersecting) {
                            observer.unobserve(entry.target);
                            searchByBudgetDataLayer(entry.target, "view_promotion");
                        } 
                    });
                });

                if (document.querySelector(".searchByBudget")) {

                    let searchByBudgets = document.querySelectorAll(".searchByBudget");
                    for (const searchByBudget of searchByBudgets) {
                        observer.observe(searchByBudget);

                        searchByBudget.addEventListener("click", (event) => {
                            searchByBudgetDataLayer(event.currentTarget)
                            console.log(event.currentTarget)
                        });
                    }
                }


            })//trip finder - data layer
        </script>



    
