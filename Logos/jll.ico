        <script id="eloquaFormScript" type="text/javascript" language="JavaScript"><!--//
		var domain='email-am.us.jll.com';
		sessionStorage.setItem("eloqua-siteId", '65254455');
        _elqQ.push( [ 'elqSetSiteId', '65254455' ] );
        if(domain){
          sessionStorage.setItem("eloqua-domain", domain);
          _elqQ.push(['elqUseFirstPartyCookie', domain]);
        }
        _elqQ.push( [ 'elqTrackPageView' ] );
               
        window.JLLEloqua = ( function( g ) {

            var contactDataPromise;
            var customerGuidPromise;
            var visitorDataLookupId = '18ab9fab9ccd4d10a54a39e4ede20d44';

            var eloquaFunctions = {
                getContactData: function( field, contactDataLookupId ) {
                    return eloquaFunctions.getContactDataPromise( contactDataLookupId )
                        .then( function() {
                            return GetElqContentPersonalizationValue( field );
                        } );
                },
                getContactDataPromise: function( contactDataLookupId ) {
                    // We don't do a data lookup unless something needs it
                    if ( !contactDataPromise ) {
                        contactDataPromise = new Promise( function( resolve, reject ) {

                            var contactDataLookupComplete = function() {
                                resolve();
                            };

                            var visitorDataLookupComplete = function() {
                                g.SetElqContent = contactDataLookupComplete;
                                setTimeout( function(){resolve()}, 2000);

                                g._elqQ.push( [ 'elqDataLookup', escape( contactDataLookupId ),
                                    '<C_EmailAddress>' + GetElqContentPersonalizationValue( 'V_ElqEmailAddress' ) + '</C_EmailAddress>' ] );
                            };

                            if ( visitorDataLookupId && contactDataLookupId ) {
                                g._elqQ.push( [ 'elqDataLookup', escape( visitorDataLookupId ), '' ] );
                            } else {
                                console.log( 'Eloqua data lookup requested but no lookup ID provided' );
                            }

                            g.SetElqContent = visitorDataLookupComplete;
                        } );
                    }

                    return contactDataPromise;
                },
                getCustomerGUID: function() {
                    if ( !customerGuidPromise ) {
                        g._elqQ.push( ['elqGetCustomerGUID'] );

                        customerGuidPromise = new Promise( function( resolve, reject ) {

                            var tryToGetCustomerGuid = function( retryAttempts ) {
                                if ( retryAttempts > 0 ) {
                                    if ( typeof g.GetElqCustomerGUID === 'function' ) {
                                        return resolve( GetElqCustomerGUID() );
                                    }
                                    setTimeout( function() { tryToGetCustomerGuid( retryAttempts - 1 ) }, 550 );
                                } else {
                                    reject();
                                }
                            };

                            tryToGetCustomerGuid( 3 );

                        } );
                    }

                    return customerGuidPromise;
                }
            };

            return eloquaFunctions;

        } )( window );



        //--></script>
    

        <script type="text/javascript">
            ( function() {

                $( document ).on( 'searchQueryIssued', function( event ) {
                    $.ajax( '/bin/jll/powersearch', {
                        method: "GET",
                        data: {
                            "query": event.detail.query
                        }
                    } )
                        .then( function( result ) {
                            $( document ).trigger( {
                                type: "propertyListingSearchCompleted",
                                detail: ( result.listings || [] ).slice( 0, event.detail.pageSize || 3 )
                                    .map( function( listing ) {
                                        return {
                                            name: listing.propertyName,
                                            address: {
                                                addressLocality: listing.city,
                                                addressRegin: listing.state,
                                                postalCode: listing.zip
                                            },
                                            assetTypes: ( listing.usageTypes || [] ).map( function( type ) {
                                                return {
                                                    name: type
                                                };
                                            } ),
                                            tenures: ( listing.tenureTypes || [] ).map( function( type ) {
                                                return {
                                                    type: type
                                                };
                                            } ),
                                            url: "https://powersearch.jll.com/us-en/property/{propertyId}/{seoName}"
                                                .replace( '{propertyId}', listing.propertyId )
                                                .replace( '{seoName}', listing.seoName )
                                        };
                                } ) } );
                        }, function( error ) {
                            $( document ).trigger( {
                                type: "propertySearchQueryFailure",
                                error: error
                            } );
                        } )
                } );

            } )();
        </script>






    
    
    



    
