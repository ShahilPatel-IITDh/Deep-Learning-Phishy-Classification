    // SOURCE: https://github.com/tagticians/segment-onetrust-integration-cookie
    !function(){
      function getCookie(name) {
        var value = '; ' + document.cookie;
        var parts = value.split('; ' + name + '=');
        if (parts.length === 2) return parts.pop().split(';').shift();
      }
    
      function registerAndCall() {
          // No Cookie Failsafe
          var consentOnetrust = '';
          var consentPreferences = {
            "Performance": false,
            "Targeting": false,
            "Functional": false,
            "Engagement": false
          };
          var consentGoogle = {};
         
          // Grab OptanonConsent cookie value
          var cookieOptanonConsent = getCookie('OptanonConsent');
          if (typeof cookieOptanonConsent !== 'undefined') {
              cookieOptanonConsent = decodeURIComponent(cookieOptanonConsent);
              // Get 'groups' from OptanonConsent cookie
              if (cookieOptanonConsent.indexOf('&groups=') > -1) {
                  consentOnetrust = cookieOptanonConsent.split('&groups=')[1].split('&')[0];
              }
          }
          
          if (window.OnetrustActiveGroups) {
            consentPreferences["Performance"] = window.OnetrustActiveGroups.includes("C0002");
            consentPreferences["Functional"] = window.OnetrustActiveGroups.includes("C0003");
            consentPreferences["Targeting"] = window.OnetrustActiveGroups.includes("C0004");
            consentPreferences["Engagement"] = window.OnetrustActiveGroups.includes("C0005");
            consentGoogle = {
              ad_storage: !!consentPreferences["Targeting"] ? "granted" : "denied",
              analytics_storage: !!consentPreferences["Performance"] ? "granted" : "denied",
            };
          }
      
          // Register OneTrust Integration plugin
          window.analytics.register({
              name: 'OneTrust Integration Cookie',
              version: '0.1.0',
              type: 'enrichment',
              isLoaded: () => true,
              load: () => Promise.resolve(),
              // context object and reference to the analytics.js instance
              page: (ctx) => {
                  ctx.updateEvent(ctx.event.context.consent = { 'onetrust': consentOnetrust, 'consentPreferences': consentPreferences, 'google': consentGoogle });
                  return ctx;
              },
              track: (ctx) => {
                  ctx.updateEvent(ctx.event.context.consent = { 'onetrust': consentOnetrust, 'consentPreferences': consentPreferences, 'google': consentGoogle });
                  return ctx;
              },
              identify: (ctx) => {
                  ctx.updateEvent(ctx.event.context.consent = { 'onetrust': consentOnetrust, 'consentPreferences': consentPreferences, 'google': consentGoogle });
                  return ctx;
              },
          });
          
          var excludedUserAgents = [
            'Googlebot',
            'Bingbot',
            'Slurp',
            'DuckDuckBot',
            'Baiduspider',
            'Datadog'                       
          ];
          
          window.analytics.register({
              name: 'LegalZoom Bot Filter',
              version: '1.0.0',
              type: 'before',
              isLoaded: () => true,
              load: () => Promise.resolve(),
              // context object and reference to the analytics.js instance
              page: (ctx) => {
                  if(excludedUserAgents.some((ua) => navigator.userAgent.includes(ua))) {
                    return ctx.cancel();
                  } else {
                    return ctx;
                  }
              },
              track: (ctx) => {
                  if(excludedUserAgents.some((ua) => navigator.userAgent.includes(ua))) {
                    return ctx.cancel();
                  } else {
                    return ctx;
                  }
              },
              identify: (ctx) => {
                  if(excludedUserAgents.some((ua) => navigator.userAgent.includes(ua))) {
                    return ctx.cancel();
                  } else {
                    return ctx;
                  }
              }
          });
      
          // Send Pageview
          window.analytics.page(); 
      }
      window.analytics.ready(() => {
        registerAndCall();
      });
  
