<script>
    (function() {
        function $c(name, attr) {
            const elem = document.createElement(name);
            if (attr) {
                Object.keys(attr).forEach((key) => {
                elem.setAttribute(key, attr[key]);
                });
            }
            return elem;
        }

        const script = $c('script', { src: '/resources/js/ldclient-3.1.3.min.js' });
        script.addEventListener('load', () => {
            // Set clientSideID to your LaunchDarkly client ID
            const clientSideID = (window.drupalSettings && window.drupalSettings.launchdarkly_martech_client_id)
                ? window.drupalSettings.launchdarkly_martech_client_id
                : (window.launchdarkly_client_id)
                    ? window.launchdarkly_client_id
                    : '645e5a0d392206121fed3044' //MarTech Prod

            const queryParams = new URLSearchParams(window.location.search);
            const user = {
                key: 'site-external-resources',
                custom: {
                    queryParam: queryParams.get('launchdarkly'),
                },
            };

            const ldclient = LDClient.initialize(clientSideID, user);

            ldclient.on('initialized', () => {
                // eslint-disable-next-line no-console
                console.info('SDK successfully initialized!');
            });

            ldclient.on('failed', () => {
                // eslint-disable-next-line no-console
                console.info('SDK failed to initialize');
            });


            ldclient.on('ready', () => {
                renderDA(ldclient.variation('show-doc-assist', false));
            });

            ldclient.on('change', (status) => {
                if (status['show-doc-assist']) {
                    renderDA(status['show-doc-assist'].current);
                }
            });
        });
        document.body.appendChild(script);

        function insertDA(afterNode) {
            const li = $c('li');
            const a = $c('a', {
              href: '/ai/doc-assist',
              title: 'Doc Assist',
              class: 'ga-hook amp-hook',
              'data-ga-category': 'lzred_global_header',
              'data-ga-label': 'global_header_legal_help_doc_assist',
              'data-amplitude-property': 'global navigation interaction',
              'data-amplitude-value': 'doc assist',
            });
            const badge = $c('span', {
              style: 'display: inline-block; border-radius: .25rem; padding: .125rem .5rem; font-size: .75rem; line-height: 1rem; background: #fff9eb; color: #ab7800; font-weight: 500; vertical-align: text-bottom;',
            });
            badge.appendChild(document.createTextNode('BETA'));
            a.appendChild(document.createTextNode('Doc Assist '));
            a.appendChild(badge);
            li.appendChild(a);
            afterNode.parentElement.insertBefore(li, afterNode);
        }

        function renderDA(flagValue) {
            const legalForms = document.querySelector('[data-ga-label="global_header_legal_legal_forms"]').parentElement;
            const docAssist = document.querySelector('[data-ga-label="global_header_legal_help_doc_assist"]');
            if (flagValue && !docAssist) {
                insertDA(legalForms);
            } else if (docAssist) {
                docAssist.parentElement.remove();
            }
        }

    })();
</script>
