<script>
var $ = jQuery;
//build the tabs markup
$('.tab-item').each(function(i){
  if(i == 0){ $(this).before('<div class="tools-switcher"></div>') }
  let name = $(this).find('p').first().text();
  let icon = $(this).find('.n1-icon').detach();
  let btn = '<button data-index="'+i+'">'+ name +'</button>';
  $('#tools').find('.tools-switcher').append(btn);
  $('#tools').find('button').last().prepend(icon);
});
//toggle state
let init = true;
$('#tools').on('click', '.tools-switcher', function(e){
  let $switcher = $('#tools').find('.tools-switcher');
  let isButton = $(e.target)[0].nodeName == 'BUTTON' ? true : false;
  e.preventDefault();
  if($(window).outerWidth() < 671){
    if(!init){
      if(!$switcher.hasClass('open')){ if(isButton){ let x = $(e.target).detach();$switcher.prepend(x);} }
      $switcher.toggleClass('open');
    }
  }else{ init = false; }
  if(isButton){
    let index = $(e.target).attr('data-index');
    $('#tools').find('.active').removeClass('active');
    $(e.target).addClass('active');
    $('.tab-item').eq(index).addClass('active');
  } 
});
$('#tools').find('button').first().click();
</script>



