            $.ajax({
                type: "POST",
                url: "/home/View_cart_floating",
                async: false,
                contentType: "application/json; charset=utf-8",
                success: function (response) {
                    if (response != "") {

                        let tabla = JSON.parse(response);
                        let items = [];
                        let item = {};

                        for (var i = 0; i < tabla.length; i++) {

                            let sku = tabla[i].stsku;
                            let producto = tabla[i].stProducto;
                            let categoria = tabla[i].stSeccion;
                            let precio = tabla[i].dPrecio;
                            let cantidad = tabla[i].iCantidad;

                            item = {
                                item_id: sku.trim(),
                                item_name: producto.trim(),
                                item_category: categoria.trim(),
                                price: precio,
                                quantity: cantidad
                            }
                            items.push(item);
                        }

                        window.dataLayer = window.dataLayer || [];
                        dataLayer.push({ ecommerce: null });
                        dataLayer.push({
                            event: 'view_cart_floating',
                            ecommerce: {
                                items: items
                            }
                        })
                    }
                }
            })
        } catch (e) {

        }
    }

    function closeRightMenu() {
        document.getElementById("rightMenu").style.display = "none";
       
    }
