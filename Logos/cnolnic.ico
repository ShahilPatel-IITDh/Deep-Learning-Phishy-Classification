<script >
	$(document).ready(function(){
		$(":text").maxlength();
		$("#domain_in").support_check();
		$("#domain_in").keydown(function(e){
			event_global=e;
			if(e.which == 13){
				query_domain();
			}
		});
		// 选择框绑定点击事件
		$(":checkbox").change(function()
		{
			check_single($(this).attr('id'));
		});
		$("#oDiv").mouseover(function(){
			$("#div_suffix").show();
		});
		$("#oDiv").mouseout(function(){
			$("#div_suffix").hide();
		});
	});
	function onbox(obj)
	{
		$(".box").removeClass("hover");
		$(obj).addClass("hover");
	}
	function load_ajax()
	{
		$(":checkbox").each(function()
		{
			if($(this).is(":checked")==false)
				$(this).parent().attr("class","");
			else
				$(this).parent().attr("class","checked");
		});		
		$("#ajaxform").submit();
	}
	$(function(){
		var options = {
			success:  function(data){
				var arr=data.split('|@^#@|');
				arr[0]=parseInt(arr[0]);
				if(arr[0]==0)
				{
					$("#tjykj").after(arr[1]);
					$("#tjykj").remove();
					$("#qzjj").after(arr[2]);
					$("#qzjj").remove();
					$("#ydtj").after(arr[3]);
					$("#ydtj").remove();
					$("#yhhd").html(arr[4]);
					$("#ggtz").html(arr[5]);
					$("#hyzx").html(arr[6]);
				}
				else
				{
				}
			},  //处理完成
			error:function(){
			}  
		}
		$('#ajaxform').ajaxForm(options);
	});
	function query_domain()
	{
		if($("#domain_in").attr("value").replace(/(^\s*)|(\s*$)/g,"")=="")
		{
			alert_dialog_zzy("提示信息","请输入您要查询的域名",1);
			return;
		}
		var suffix="";
		if(escape($("#domain_in").attr("value")).indexOf("%u")>=0)
		{
			$("#type").attr("value", "2");
			suffix=".中国|.公司|.网络|.网址|.网址|.集团|.在线|.中文网|.游戏|.商店|.网站|.企业|.购物|.娱乐|.移动|.com|.cn |.top|.xyz|.art|.club|.wang|.shop|.vip|.net|.biz|.ltd|.group|.site|.world|.company";
		}
		else
		{
			$("#type").attr("value", "1");
			$("#suffix_a").find(":checkbox").each(function(){
				if($(this).is(":checked")==true)
				{
					if(suffix==0)
					{
						suffix=$(this).siblings("a").html();
					}
					else
					{
						suffix=suffix+"|"+$(this).siblings("a").html();
					}
				}
			});
		}
		$("#suffix").attr("value",suffix);
		//	$("#domain").attr("value",$("#domain_in").attr("value"));
		$("#domainform").submit();
	}
	function close_tt()
	{
		document.cookie = "close_ad=true";
		$("#ad").attr("style","display:none");
	}
</script>

