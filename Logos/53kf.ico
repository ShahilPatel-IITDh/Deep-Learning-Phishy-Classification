                $(this).siblings("input").css({
                    "borderColor": "#218BFC"
                });
                var url = '/reg/index?phonenumber='+val+'&url='+window.location.href+'&'+arg;
                window.open(url, "_self");
            } else{
                $(this).parents(".zhuce-both").find(".input-phone").css({
                    "border": "2px solid #F44024"
