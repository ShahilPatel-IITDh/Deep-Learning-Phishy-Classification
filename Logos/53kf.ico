<script>
	//手机号码判断
	// 输入手机号
	//   var mobileReg = /(1\d{0,10}|1)/;// 手机号输入矫正表达式
	var mobileReg = /(1[3-9]\d{0,9}|1)/;// 手机号输入矫正表达式
	$("body").on("keyup", ".input-phone,.input-phonecall,.register_phone", function (e) {
			// 初始化去空格
			var keyup_value = $(this).val().replace(/\s/g, '');
			// console.log("keyup_value:", keyup_value)
			var newValue = '';
			// 输入矫正
			newValue = mobileReg.exec(keyup_value) !== null ? mobileReg.exec(keyup_value)[0] : "";
			// 输入添加空格间隔符
			newValue = newValue.toString().replace(/(.{3})/, '$1 ');
			newValue = newValue.toString().replace(/(.{8})/, '$1 ');
			// 判断删除状态时去掉字符串末尾空格
			if (e.keyCode == 8 || e.keyCode == 46) {
					if (newValue[newValue.length - 1] === ' ') newValue = newValue.slice(0, newValue.length - 1);
			}
			// 展示格式化过的手机号
			$(this).val(newValue);
	})
	$(".zhuce-btn").click(function() {
            //获取兄弟节点中的input节点
            var val;
            if($(this).text() == "免费试用"){
                val =  $(this).parents(".zhuce-both").find(".pcinput").val();
            }else if($(this).text() == "创建呼叫中心"){
                val =  $(this).parents(".zhuce-both").find(".mbinput").val();
            }else if($(this).text() == "我要免费试用"){
								val =  $(this).parents(".zhuce-both").find(".input-phone").val();
						}
						else{
                val =  $(this).parents(".zhuce-both").find(".pcinput").val();
            }
            
            var reg_blank = new RegExp(' ', "g")
            if(val){
                val = val.replace(reg_blank, "");
            }
								console.log(val,1111);
           
            var reg = /^1[3-9]\d{9}$/;
            var arg = $(this).attr("data-from")
            if (reg.test(val)) {
                var talk_page_tmp = window.location.href;
                if (talk_page_tmp.indexOf("hz6d{") != -1) {
                    talk_page_tmp = talk_page_tmp.substring(
                        0,
                        talk_page_tmp.indexOf("hz6d{")
                    );
                }
                $.ajax({
                    url: "/lword_phone",
                    data: {
                        phone: val,
                        hz6d_from_page: hz6d_from_page,
                        hz6d_land_page: hz6d_land_page,
