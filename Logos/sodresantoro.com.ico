			e.preventDefault;

			var self = $(this);
			var parentH3 = self.closest("h3");
			$(".menu-lateral-item").slideUp();
			$(".expand-filters").children('span').removeClass('fa-minus').addClass('fa-plus');

			if(!parentH3.next().is(":visible")){
				self.next().children().removeClass('fa-plus').addClass('fa-minus');
				parentH3.next().slideDown();
			}else{
				self.next().children().removeClass('fa-minus').addClass('fa-plus');
				parentH3.next().slideUp();
			}
		});

		$(".expand-filters").on("click",function(){
			$(this).prev().trigger('click');
		});

	});
</script>


