	<script>
		
		function getQueryString() {
			var query = location.search.slice(1);
			var partes = query.split('&');
			var data = {};

			partes.forEach(function (parte) {
				var chaveValor = parte.split('=');
				var chave = chaveValor[0];
				var valor = chaveValor[1];
				data[chave] = valor;
			});

			return data;
		}

		function saveFirebase(hash, url) {

			var config = {
				apiKey: "AIzaSyBSp-dvbvB_DCxLtDuhNDefF6tUWbfoTWw",
				authDomain: "navegacaosite.firebaseapp.com",
				databaseURL: "https://navegacaosite.firebaseio.com",
				projectId: "navegacaosite",
				storageBucket: "navegacaosite.appspot.com",
				messagingSenderId: "730423037258"
			};
			firebase.initializeApp(config);

			var data = getDate();
			// firebase.database().ref("/" + hash + '/' + data).push({
			firebase.database().ref("/" + hash + '/' + data).set({
				url: url,
			});
		}

		function getDate() {
			var dataAtual = new Date();
			var dia = dataAtual.getDate();
			var mes = ('00' + (dataAtual.getMonth() + 1)).slice(-2);
			var ano = dataAtual.getFullYear();
			var hora = ('00' + dataAtual.getHours()).slice(-2);
			var minuto = ('00' + dataAtual.getMinutes()).slice(-2);
			var segundo = ('00' + dataAtual.getSeconds()).slice(-2);

			return dia + '-' + mes + '-' + ano + '_' + hora + ":" + minuto + ":" + segundo;
		}

		function saveNavigationEstadao() {
			var value = document.cookie;
			var queryString = getQueryString();

			if (value.indexOf('navigation') > -1) {
				var parts = value.split(";");
				for (var i in parts) {
					if (parts[i].indexOf('navigation') > -1) {
						var hash = parts[i].split("=")[1];
					}
				}
			} else if (queryString.ref === 'estadao') {
				var hash = 'user_' + getDate() + '_' + parseInt(Math.random() * 1000);
				document.cookie = "navigation=" + hash;
			} else {
				return false;
			}

			var url = window.location.href;
			saveFirebase(hash, url);
		}

		saveNavigationEstadao();
		
	</script>

