        created() {
            let count = Math.floor(this.noticedList.length / 3)
            if (count * 3 < this.noticedList.length) {
                count += 1;
            }
            let arr = []
            for (let i = 0; i < count; i++) {
                let formIdx = i * 3;
                let toIdx = (i + 1) * 3;
                if (toIdx > this.noticedList.length) {
                    toIdx = this.noticedList.length
                }
                arr.push(this.noticedList.slice(formIdx, toIdx))
            }
            this.noticedList = arr
        },
        mounted() {
            let that = this;
            setInterval(() => {
                that.getHotBidding()
            }, 10000)

            new Swiper('.swiper-container', {
                pagination: '.swiper-pagination',
                paginationClickable: true,
                autoplayDisableOnInteraction : false, //手动播放后继续自动播放
                speed: 600,
                autoplay: 3000,
                loop: true
            });
        },
        methods: {
            getHotBidding() {
                let that = this;
                api.bid.hotBidList(9).then(res => {
                    const {result, data} = res
                    if (result) {
                        that.domainBiddingList = data
                    }
                })
            },
            choseSearch(type) {
                this.searchType = type
            },
            showBarrage(idx) {
                if (idx === this.barrageIndex) {
                    this.barrageIndex = -1
                } else {
                    this.barrageIndex = idx
                }
            },
            search() {
                var _this = this;
                if (this.searchType === 0) {
                    window.location.href = 'dmSearch?type=0&kw=' + _this.searchKey
                } else {
                    // 查whois
                    window.location.href = "whois?type=1&domainName=" + _this.searchKey
                }
            }
        },
        filters: {
            dateFilter(v) {
                let datetime = new Date(v);
                let year = datetime.getFullYear();
                let month = datetime.getMonth() + 1 < 10 ? '0' + (datetime.getMonth() + 1) : datetime.getMonth() + 1;
                let date = datetime.getDate() < 10 ? '0' + datetime.getDate() : datetime.getDate();
                return `${year}-${month}-${date}`
            },
            timeFilter(v) {
                let str = '';
                if (v == null) {
                    str = '竞价结束';
                } else {
                    let t = Math.floor((new Date(v).getTime() - new Date().getTime()) / 1000);
                    if (t <= 0) {
                        str = '竞价结束';
                    } else {
                        let d = Math.floor(t / (24 * 3600));
                        let h = Math.floor((t - d * 24 * 3600) / 3600);
                        let m = Math.floor((t - d * 24 * 3600 - h * 3600) / 60);
                        let s = Math.floor((t - d * 24 * 3600 - h * 3600 - m * 60));

                        if (d > 0) {
                            if (d < 10) {
                                d = '0' + d;
                            }
                            str += `${d}天`
                            if (h > 0) {
                                if (h < 10) {
                                    h = '0' + h;
                                }
                                str += `${h}时`
                            }
                            return str
                        } else {
                            if (h > 0) {
                                if (h < 10) {
                                    h = '0' + h;
                                }
                                str += `${h}时`
                                if (m > 0) {
                                    if (m < 10) {
                                        m = '0' + m;
                                    }
                                    str += `${m}分`
                                }
                                return str
                            } else {
                                if (m > 0) {
                                    if (m < 10) {
                                        m = '0' + m;
                                    }
                                    str += `${m}分`
                                    if (s > 0) {
                                        if (s < 10) {
                                            s = '0' + s;
                                        }
                                        str += `${s}秒`
                                    }
                                    return str
                                } else {
                                    if (s > 0) {
                                        if (s < 10) {
                                            s = '0' + s;
                                        }
                                        str += `${s}秒`
                                    }
                                    return str
                                }
                            }
                        }

                    }
                }
                return str;
            },
        }
    })
