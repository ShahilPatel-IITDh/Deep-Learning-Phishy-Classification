    <script type="text/javascript">

        $(document).ready(function() {
            
            $.validate({
                modules : 'security, logic',
                onSuccess: function($form) {
                    $('input[type=submit].modal-login-btn').val('Signing in. Please wait...').prop('disabled', true);
                }
            });

            var signupForm       = $('#signup-form');
            var signupFormWidth  = signupForm.outerWidth();
            var signupFormHeight = signupForm.outerHeight();
            var windowWidth      = $(window).width();
            var windowHeight     = $(window).height();
            var signupFormFixed  = true;

            $(window).resize(function() {
                windowWidth      = $(this).width();
                windowHeight     = $(this).height();
                signupFormHeight = signupForm.outerHeight();
                signupFormWidth  = signupForm.outerWidth();
                if ((signupFormHeight + 180) < windowHeight) {
                    if (windowWidth < 990) {
                        signupForm.removeClass('fixed');
                        signupForm.css('width', '100%');
                    }
                } else {
                    signupForm.removeClass('fixed');
                    signupForm.css('width', '100%');
                }
            });

            $(window).scroll(function() {

                windowHeight = $(this).height();

                if ($(this).scrollTop() > 310) {
                    $('#cta-main-nav').removeClass('hidden-sm-down');
                } else {
                   $('#cta-main-nav').addClass('hidden-sm-down');
                }
                
                if ((signupFormHeight + 180) < windowHeight) {
                    if (windowWidth > 990) {
                        if ($(this).scrollTop() > 120) {
                            signupForm.addClass('fixed');
                            signupForm.css('width', signupFormWidth);
                        } else {
                            signupForm.removeClass('fixed');
                            signupForm.css('width', '100%');
                        }
                    }
                } else {
                    if (signupFormFixed == false) {
                        signupForm.removeClass('fixed');
                    }
                }
            });

            $('body').click(function(e) {
                $('[data-toggle="popover"]').each(function () {
                    if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0) {
                        $(this).popover('hide');
                    }
                });
            });

            $('[data-toggle="popover"]').popover();
            
            $('[data-toggle="popover"]').click(function(event) {
                event.preventDefault();
            });

            function validateForm() {
                var errors = 0;
                $('input[name=full_name]').validate(function(valid, elem) {
                    if (!valid) errors++;
                });
                $('input[name=email]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('select[name=position]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('select[name=phone]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('select[name=state]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=school_name]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=suburb]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });
                $('input[name=accept_terms]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });      
                $('input[name=recaptcha]').validate(function(valid, elem) {
                   if (!valid) errors++;
                });                
                return errors > 0 ? false : true;
            }

            var template = $('#code_data');

            $('#position').change(function(e) {
                e.preventDefault();
                if ($(this).val() == 'primary_school_teacher') {
                    $('input[name=school_name]').val('');
                    $('input[name=suburb]').val('');
                    $('#toggle-school-info').addClass('hidden');
                    signupFormFixed = true;
                } else {
                    signupFormFixed = false;
                    $('#toggle-school-info').removeClass('hidden');
                    if ($(window).scrollTop() > 0) {
                        $('body,html').animate({
                            scrollTop : 0                       
                        }, 500);                      
                    }                      
                }
                signupFormHeight = signupForm.outerHeight();
            });

            $('#signup-button').click(function(event) {
                event.preventDefault();
                if (validateForm()) {
                    $(this).attr('disabled', true).val('Processing...');
                    $('#signup-form').submit();
                } else {
                    $(this).attr('disabled', false).val('Get Started');
                    windowWidth      = $(this).width();
                    windowHeight     = $(this).height();
                    signupFormHeight = signupForm.outerHeight();
                    signupFormWidth  = signupForm.outerWidth();
                    window.location.href="#top";
                    if ((signupFormHeight + 160) < windowHeight) {
                        if (windowWidth < 990) {
                            signupForm.removeClass('fixed');
                            signupForm.css('width', '100%');
                        }
                    } else {
                        signupForm.removeClass('fixed');
                        signupForm.css('width', '100%');
                    }                
                }
            });

            if ($("#homeschool-video").length > 0) {
                var player = videojs("homeschool-video");
                $('#play-demo').click(function() {
                    player.play();
                });
            }

            /* Video Library */
            var $arrVideos = [
                'AbsEYHut1hs',
                '6iIU53VdAtg',
                'Y--qUVeFVfA',
                'LL_6dCzQ_j8',
                'Z8kLr08fLTU',
                'ltHlNFiN1Xs',
                'gqyFqDzLU5Y',
                '69pACegCXiA',
                'xlojss6e7aY',
                'vj2qj885I70',
                '5FLuAENN8jc',
                'g4pw07ZCfb4',
                'MwKN5_IfGs4',
                'cIXSB3Acz14',
                'qPHwoCpxaDU',
                '8mJN1YlghNo'
            ];

            function getParameterByName(name) {
                var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
                return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
            }

            var $specifiedVideoID = parseInt(getParameterByName('index'));
            var $startPlayPosition = parseInt(getParameterByName('start'));
            
            if ($specifiedVideoID >= 0 && $specifiedVideoID < $arrVideos.length) {

                $('#support-section-tabs li:eq(0) a').tab('show');

                var $id = $arrVideos[$specifiedVideoID];
                var $title = $('.video-index-'+$specifiedVideoID).attr('data-title');
                $url = "https://www.youtube.com/embed/";
                $url = $url + $id + '?autoplay=1&rel=0';

                if ($startPlayPosition >= 0) {
                    $url += '&start='+$startPlayPosition;                    
                }
   
                $("#videoModal iframe").attr("src", $url);
                $title = $('a[data-video-index='+$specifiedVideoID+']').attr('data-title');
                
                setTimeout(function() { 
                    $('#modalVideoLabel').html($title);
                    $('#videoModal').modal('show');
                }, 500);
            }

            var centreSectionDefault = getParameterByName('section');
            var article = getParameterByName('article');

            if (centreSectionDefault) {
                switch (centreSectionDefault) {
                    case 'video':
                        $('#support-section-tabs li:eq(0) a').tab('show');
                    break;
                    case 'guide':
                        $('#support-section-tabs li:eq(1) a').tab('show');
                    break;
                    case 'downloads':
                        $('#support-section-tabs li:eq(2) a').tab('show');
                    break;         
                    default:
                        $('#support-section-tabs li:eq(0) a').tab('show');
                    break;                               
                }
            }

            if (article) {
                $.ajax({
                    type: "POST",
                    url: "/docs/teachers/load_content",
                    data: { template: article },
                    success: function(data) { 
                        if (data) {
                            $('#guide_content').html(data);
                            $('body,html').animate({
                                scrollTop : 0                       
                            }, 500);
                            $(".menu-siderbar a.list-group-item, #leftnavDropdown a.dropdown-item").each(function() {
                                var url = $(this).attr('href').split('/');
                                var urlArticle = url[3];
                                $(this).parent().children('a.dropdown-item').removeClass('active');
                                $(this).parent().children('a.list-group-item').removeClass('active');
                                if (urlArticle == article) {
                                    $(this).addClass('active');
                                }
                            });
                        } else {
                            window.location = '/docs/teachers?section=guide';
                        }
                    }
                }).fail(function( data ) {
                    window.location = '/docs/teachers?section=guide';
                });
            }

            $('.video-thumbnail').click(function(event) {
                var $id = $(this).attr('data-id');
                var $title = $(this).attr('data-title');
                var $url = "https://www.youtube.com/embed/";
                if ($id) {
                    $url = $url + $id + '?autoplay=1&rel=0';
                    $("#videoModal iframe").attr("src", $url);
                }
