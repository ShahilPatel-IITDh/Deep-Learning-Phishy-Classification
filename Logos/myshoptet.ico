            "cta" : "Folytatni shoptet.hu",
            "link" : "https://www.shoptet.hu/"
        }
    };

    var locale = [
        ...(window.navigator.languages || []),
        window.navigator.language,
        window.navigator.browserLanguage,
        window.navigator.userLanguage,
        window.navigator.systemLanguage
    ]
        .filter(Boolean)
        .map(language => language.substr(0, 2))
        .find(language => availableLanguages.includes(language));

    $(document).ready(function() {
        if (locale && locale != currentLanguage && $('.lnghint').length > 0) {
            document.getElementById('lnghinttext').innerHTML = i18n[locale].text;
            document.getElementById('hintlink').innerHTML = i18n[locale].cta;
            document.getElementById('hintlink').setAttribute('href', i18n[locale].link);
            setTimeout(() => {
                document.getElementById('main-in')?.classList.add('lnghint-active');
                $('.lnghint').slideDown(500);
            }, 1000);
        }

        $('.close').click(() => {
            document.getElementById('main-in')?.classList.remove('lnghint-active');
            $('.lnghint').slideUp(500);
            $.cookie("closeLngHint", "true", {
                expires: 14,
                path: '/'
            });
        });
    });

