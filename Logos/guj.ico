<script>
    document.addEventListener('DOMContentLoaded', function () {
        setTargetAttribute();
    });

    function setTargetAttribute() {
        var regex = new RegExp(/((http:\/\/|https:\/\/|www\.)(?!(company\.rtl\.com|dachpressezentrum-cms\.netrtl\.com|dachpressezentrum-cms-tst\.netrtl\.com))[\w\.\/\-=?#]+)/, "i");
        const allATags = document.getElementsByTagName('a');

        for (let i = 0; i < allATags.length; i++) {
            const aTag = allATags[i];
            const url = aTag.getAttribute('href');

            if (url && regex.test(url)) {
                aTag.setAttribute('target', '_blank');
            } else {
                aTag.setAttribute('target', '_self');
            }
        }
    }
</script>

