    <script>
      var scrollifyEnabled = false;
      $(function () {
        if (window.innerWidth > 768) {
          $.scrollify({
            section: '.home-page',
            interstitialSection: '',
            sectionName: 'section-name',
            easing: 'easeOutCubic',
            scrollSpeed: 600,
            offset: 0,
            scrollbars: false,
            standardScrollElements: '',
            setHeights: false,
            overflowScroll: true,
            updateHash: true,
            touchScroll: true,
            before: function (e) {
              switch (e) {
                case 0:
                  $('img#firstMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  $('img#secondMainDot, img#thirdMainDot, img#fourthMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  break;
                case 1:
                  $('img#firstMainDot, img#thirdMainDot, img#fourthMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#secondMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                case 2:
                  $('img#firstMainDot, img#secondMainDot, img#thirdMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#fourthMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                case 3:
                  $('img#firstMainDot, img#secondMainDot, img#fourthMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#thirdMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                case 4:
                  $('img#firstMainDot, img#secondMainDot, img#thirdMainDot, img#fourthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#fifthMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                default:
                  break;
              }
            },
            after: function () {
            },
            afterResize: function () {
              if (window.innerWidth < 768) {
                location.reload();
              }
            },
            afterRender: function () {
              scrollifyEnabled = true;
            }
          });
        } else {
          // $.scrollify({
          //   section: '.container-mobile',
          //   interstitialSection: '',
          //   easing: 'easeOutCubic',
          //   scrollSpeed: 600,
          //   offset: 0,
          //   scrollbars: false,
          //   standardScrollElements: '',
          //   setHeights: true,
          //   overflowScroll: true,
          //   updateHash: true,
          //   touchScroll: true,
          // });
        }
      });

      function debounce(func, wait, immediate) {
        var timeout;
        return function () {
          var context = this,
            args = arguments;
          var later = function () {
            timeout = null;
            if (!immediate) {
              func.apply(context, args);
            }
          };
          var callNow = immediate && !timeout;
          clearTimeout(timeout);
          timeout = setTimeout(later, wait);
          if (callNow) {
            func.apply(context, args);
          }
        };
      }

      function redirect(event) {
        event.preventDefault();
        window.location = event.currentTarget.getAttribute('href');
      }

      $(window).resize(debounce(function () {
        var width = $(this).width();
        if (width > 768 && !scrollifyEnabled) {
          $.scrollify({
            section: '.home-page',
            interstitialSection: '1',
            easing: 'easeOutCubic',
            scrollSpeed: 600,
            offset: 0,
            scrollbars: false,
            standardScrollElements: '',
            setHeights: false,
            overflowScroll: true,
            updateHash: true,
            touchScroll: true,
            before: function (e) {
              switch (e) {
                case 0:
                  $('img#firstMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  $('img#secondMainDot, img#thirdMainDot, img#fourthMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  break;
                case 1:
                  $('img#firstMainDot, img#thirdMainDot, img#fourthMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#secondMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                case 2:
                  $('img#firstMainDot, img#secondMainDot, img#thirdMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#fourthMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                case 3:
                  $('img#firstMainDot, img#secondMainDot, img#fourthMainDot, img#fifthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#thirdMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                case 4:
                  $('img#firstMainDot, img#secondMainDot, img#thirdMainDot, img#fourthMainDot').attr({
                    src: 'img/icons/DotUnActive.svg',
                    class: 'unactiveDot'
                  });
                  $('img#fifthMainDot').attr({
                    src: 'img/icons/DotActive.svg',
                    class: 'activeDot'
                  });
                  break;
                default:
                  break;
              }
            },
            after: function () {
            },
            afterResize: function () {
              if (window.innerWidth < 768) {
                location.reload();
              }
            },
            afterRender: function () {
              scrollifyEnabled = true;
            }
          });
        }
      }, 50));

      $(window).trigger('resize');
    </script>
