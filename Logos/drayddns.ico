<script>
    //cookie
    const
        cookieAcceptStr = "cookie=accept",
        cookieAry = document.cookie.split("; "),
        cookieContent = document.querySelector(".cookie");
    cookieContent.querySelector("a").style.textDecoration = "underline";
    if (cookieAry.indexOf(cookieAcceptStr)==-1) {
        cookieContent.style.display = "block";
        cookieContent.querySelector("button").onclick = function () {
            document.cookie = `${cookieAcceptStr}; max-age = ${60 * 60 * 24 * 30}; path=/;`
            cookieContent.remove();
        };
    }
</script>
