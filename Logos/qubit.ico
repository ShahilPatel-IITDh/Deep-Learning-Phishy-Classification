<script type="application/javascript" nonce="aH+L4r8jasx+2vCnmD+pcRJh">
   
    (async () => {
        let dataLayerListenerIsInitialized; 
        const dataLayerPushSearchValue = (term) => {
            if (typeof OneTrust !== 'undefined' && OnetrustActiveGroups.includes(',C0004,')) {
                if (typeof dataLayer !== 'undefined') {
                    let value = document.querySelector('atomic-search-box').shadowRoot.querySelector('input[type="text"]').value;

                    if (term) {
                        value = term;
                    }

                    dataLayer.push({
                        "event": "user_interaction",
                        "event_name": "search_from_nav",
                        "interaction": {
                        "search_term": value
                        }
                    });
                }
            }
        };
        const listenAndSendDataLayer = (elements) => {
            elements.input.addEventListener('keypress', (event) => {
                if (event.keyCode === 13 ) {
                    dataLayerPushSearchValue();
                }
            });
            elements.submitButton.addEventListener('click', () => {
                dataLayerPushSearchValue();
            });

            setTimeout( () => {
                const querySuggest = elements.shadowDOMSearchBox.querySelector('[part="suggestions-wrapper"]');
       
                if (querySuggest) {
                    querySuggest.addEventListener('click', () => {
                        const suggestion = querySuggest.querySelector('[part="suggestion active-suggestion suggestion-with-query recent-query-item"]')?.dataset.query;
                       
                        dataLayerPushSearchValue(suggestion);
                    });
                }
            }, 500);
           
            dataLayerListenerIsInitialized = true;
        };

        await customElements.whenDefined('atomic-search-interface');
        const searchInterface = document.querySelector('#search-box');
        const platformUrl = 'https://coveosearch.org.coveo.com';
        const organizationId = 'coveosearch';
        const apiKey = 'xxa94ed2e6-f913-4e0b-bdc3-bfb9b52e969b';

        await searchInterface.initialize({
            accessToken: apiKey,
            organizationId,
            platformUrl,
            preprocessRequest: (request, clientOrigin) => { 
                const body = JSON.parse(request.body);
               
                body.tab = "all";
                
                request.body = JSON.stringify(body);
            
                 if (!dataLayerListenerIsInitialized) {
                    let elements = {};
                    elements.submitButton = document.querySelector('atomic-search-box').shadowRoot.querySelector('button');
                    elements.input = document.querySelector('atomic-search-box').shadowRoot.querySelector('input[type="text"]');
                    elements.shadowDOMSearchBox = document.querySelector('atomic-search-box').shadowRoot;
 
                    listenAndSendDataLayer(elements); 
                 } 
                return request;
            },
        });

         if (typeof OnetrustActiveGroups !== 'undefined') {
            if (OnetrustActiveGroups.indexOf('C0002') <= -1 && typeof searchInterface !== 'undefined') {
                searchInterface.analytics = false;
            }
         }
        
        const pageLang = document.documentElement.lang;

        if (pageLang) {
            searchInterface.language = pageLang;
        }

        searchInterface.i18n.addResourceBundle(pageLang, 'translation', {
            'search' : 'What are you looking for?',
        });

        const coveoAnalyticsKey = '__coveo.analytics.history';
        const coveoAnalyticsRaw = window.localStorage.getItem(coveoAnalyticsKey);

        try {
            const dateNow = Date.now();
            const coveoAnalytics = JSON.parse(coveoAnalyticsRaw);
            const href = window.location.origin + window.location.pathname;

            coveoAnalytics.unshift({
                name: "PageView",
                value: href,
                time: new Date(dateNow).toISOString(),
                internalTime: dateNow
            });

            window.localStorage.setItem(coveoAnalyticsKey, JSON.stringify(coveoAnalytics));           
        } catch (err) {
            console.warn('Could not parse Coveo Analytics');
        }
        
    })();
  
</script>

