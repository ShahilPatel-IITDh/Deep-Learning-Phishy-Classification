<script>
$(function() {
            $(window).scroll(function() {
            documentHeight = $(document).height();
            scrollPosition = $(this).height() + $(this).scrollTop();
            footerHeight = 50 + $(".l-main__footer").innerHeight() + $("footer").innerHeight();
            
            if (documentHeight - scrollPosition <= footerHeight && $(this).scrollTop() > 3500) {
            $(".floating-banner").addClass('lock');
            $(".floating-banner").removeClass('hide');
          }else if ($(this).scrollTop() < 3500){
            $(".floating-banner").addClass('hide');
          }else{
