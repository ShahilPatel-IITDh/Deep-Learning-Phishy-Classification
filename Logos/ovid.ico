	function waitForCookieBanner(selector, callback){
	  var timesRun = 0;
	  var waitInterval = setInterval(function(){
		  timesRun += 1;
		  if(timesRun === 500){
			  clearInterval(waitInterval);
		  }
		  if(document.querySelector(selector)){
			  clearInterval(waitInterval);
			  callback();
		  }
	  }, 10);
	}
