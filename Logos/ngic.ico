    const targetDiv = document.getElementById("quoteModalRSQ");
    const btnRSQ = document.getElementById("showRSQ");
    btnRSQ.onclick = function () {
        if (targetDiv.style.display !== "none") {
            targetDiv.style.display = "none";
        } else {
            targetDiv.style.display = "block";
        }
    };


	function getEnv() {
		if (window.location.hostname.toLowerCase() === 'localhost') {
			result = 'localhost';
		} else if (window.location.hostname.toLowerCase() === 'qa.nationalgeneral.com') {
			result = 'qa';
		} else if (window.location.hostname.toLowerCase() === 'stage.nationalgeneral.com') {
			result = 'stage';
		} else if (window.location.hostname.toLowerCase() === 'staging.nationalgeneral.com') {
			result = 'stage';
		} else {
			result = 'prod';
		}
		return result;
	}

	env = getEnv();
	var hrefRouting = 'https://routing.nationalgeneral.com';
	var hrefSBA = 'https://customer.nationalgeneral.com/SmallBusinessAuto/QuoteStart/SearchQuote';
	var hrefHO = 'https://customer.nationalgeneral.com/HomeInsurance/QuoteStart/SearchQuote';

    if (env == 'localhost' || env == 'qa') { 
		hrefRouting = 'https://routingqa.nationalgeneral.com'; 
		hrefSBA = 'https://customerqa.nationalgeneral.com/SmallBusinessAuto/QuoteStart/SearchQuote';
		hrefHO = 'https://customerqa.nationalgeneral.com/HomeInsurance/QuoteStart/SearchQuote';		
	}
    if (env == 'stage') { 
		hrefRouting = 'https://routingstage.nationalgeneral.com'; 
		hrefSBA = 'https://customerstage.nationalgeneral.com/SmallBusinessAuto/QuoteStart/SearchQuote';
		hrefHO = 'https://customerstage.nationalgeneral.com/HomeInsurance/QuoteStart/SearchQuote';		
	}

	document.getElementById("RSQAuto").href = hrefRouting;
	document.getElementById("RSQRV").href = hrefRouting;
	document.getElementById("RSQMC").href = hrefRouting;
	document.getElementById("RSQHO").href = hrefHO;
	document.getElementById("RSQCondo").href = hrefHO;
	document.getElementById("RSQRenters").href = hrefHO;

</script>


