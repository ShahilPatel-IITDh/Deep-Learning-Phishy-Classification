html {
font-size: 100%; 
font-family: 'Varela Round',sans-serif !important;
}

.tabs .tab a {
	color: #00bcd4;
}

.application_button {
	display: none !important;
}

#respond {
	display: none;
}

#comments {
	 padding: 0;
}

.tabs .tab a:hover, .tabs .tab a.active {
	background-color: transparent;
    color: #00bcd4; 
	
}

.woocommerce-breadcrumb, .tabs wc-tabs, #tab-description h2 {
	display: none;
}
.entry-title{
	font-size: 3rem;
}

.sub-menu .menu-item {
	float: none;
}

.details-descricao{
	width: 70%;
	padding-bottom: 10%;
	padding-top: 3%;
}

#sidebar-menu{
display: block;
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    transition-property: transform;
    transition-duration: .4s;
    background-color: #303030;
    z-index: 10000000;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
    padding: 100px 20px 80px;
    visibility: hidden;
    opacity: 0;
    -webkit-transform: scale(0.8);
    transform: scale(0.8);
    -webkit-transition: -webkit-transform .2s,opacity .2s,visibility 0s .2s;
    transition: transform .2s,opacity .2s,visibility 0s .2s;
    text-align: center;
}

.sidebar-is-visible{
	visibility: visible;
    opacity: 1;
    -webkit-transform: scale(1);
    transform: scale(1);
    -webkit-transition: -webkit-transform .4s,opacity .4s;
    transition: transform .4s,opacity .4s;
}

.woocommerce ul.products li.product .button, .woocommerce ul.products li.product .onsale, .woocommerce span.onsale, .woocommerce #respond input#submit.alt, .woocommerce a.button.alt, .woocommerce button.button.alt, .woocommerce input.button.alt {
