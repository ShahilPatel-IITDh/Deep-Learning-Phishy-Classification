body,
body.theme--default {
  --primaryText: #0050AA;
  --primaryBackground: #FAFAFA;
  --startpageBackground: var(--primaryBackground);
  --grayDarkerOrLigher: #353B42;
  --brandBaseOrLight: #0050AA;
  --brandDarkerOrLighter: #003673;
  --grayLigherOrBlack: #E3E4E5;
  --basicBlackOrWhite: #000000;
  /* pill colors */
  --pillBackground: #FFFFFF;
  --pillActiveBackground: #0050AA;
  --pillText: #353B42;
  --pillActiveText: #FFFFFF;
  --pillNavigationBackground: #0050AA;
  --pillNavigationBackgroundActive: #003673;
  --bodyBackground: #FAFAFA;
}
body[data-isonecxvariant="true"],
body.theme--default[data-isonecxvariant="true"] {
  --startpageBackground: #FFFFFF;
  --bodyBackground: #FFFFFF;
}
body.theme--blackdealsdays {
  --primaryText: #FAFAFA;
  --primaryBackground: #1B1F22;
  --startpageBackground: var(--primaryBackground);
  --grayDarkerOrLigher: #E3E4E5;
  --brandBaseOrLight: #ADC7E4;
  --brandDarkerOrLighter: #F5F8FC;
  --grayLigherOrBlack: #000000;
  --basicBlackOrWhite: #FFFFFF;
}
@media (min-width: 960px) {
body.theme--blackdealsdays .AStartPageTemplate {
    background: url('/c/img/black-friday-bg.jpeg') no-repeat, var(--startpageBackground);
    background-size: contain;
}
}
body.theme--blackdealsdays .ATheCampaign__Wrapper {
  background: var(--primaryBackground);
}
body.theme--blackdealsdays .ATheContentPage__Wrapper {
  color: var(--primaryText);
}
body.theme--parksidegreen {
  --primaryText: #0050AA;
