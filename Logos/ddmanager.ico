  function onYouTubeIframeAPIReady() {
    player1 = new YT.Player('ytplayer1', {
      height: '195',
      width: '345',
      videoId: 'loGVTyYbUKU',
      events: {
        'onReady': onPlayer1Ready
      }
    });

    player2 = new YT.Player('ytplayer2', {
      height: '195',
      width: '345',
      videoId: 'loGVTyYbUKU',
      events: {
        'onReady': onPlayer2Ready
      }
    });

    function onPlayer1Ready(event) {
      if (currentPlayer === 'ytplayer1') {
        player1.playVideo();

        return;
      }

      player1.pauseVideo();
    }

    function onPlayer2Ready(event) {
      if (currentPlayer === 'ytplayer2') {
        player2.playVideo();

        return;
      }

      player2.pauseVideo();
    }
  };

  function play(playerId) {
    (function (d, script) {
      script = d.createElement('script');
      script.type = 'text/javascript';
      script.async = true;
      script.src = 'https://www.youtube.com/iframe_api';
      d.getElementsByTagName('head')[0].appendChild(script);
    }(document));

    document.querySelector('#ytplayer1').removeAttribute('onclick');
    document.querySelector('#ytplayer2').removeAttribute('onclick');
    currentPlayer = playerId;
  }
</script>
</footer>
</body>
</html>
